<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="ring-package" namespace="/ring" extends="struts-default">
	<result-types>
		<result-type name="json" class="org.apache.struts2.json.JSONResult" />
	</result-types>
	
	<interceptors>
		<interceptor name="json" class="org.apache.struts2.json.JSONInterceptor" />
	</interceptors>
     
     <action name="getMommyRingInfo" class="com.kybaby.newbussiness.mommyring.main.GetMommyRringInfo">
       		<result name="success" type="json"></result>
       		<result name="fail" type="json"></result>
    </action>
    <action name="mommyRingManage" class="com.kybaby.newbussiness.mommyring.main.MommyRringManage">
       		<result name="success" type="json"></result>
       		<result name="fail" type="json"></result>
    </action>
     
	</package>
</struts>