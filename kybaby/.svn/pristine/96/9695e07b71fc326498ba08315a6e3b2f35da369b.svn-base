<!doctype html>
<html class="no-js" lang="zh-cn">
<head>
    <title>详情评估</title>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1"/>
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon"/>
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" href="css/evaluate.css"/>
    <link rel="stylesheet" href="css/evaluate_fd_assessment_information.css"/>
    <script src="js/modernizr-2.8.3.min.js"></script>
    <script src="js/jquery-1.11.3.min.js"></script>
    <script src="js/config.js"></script>
    <script src="js/evaluate_fd_assessment_information.js"></script>
    <!------------时间控件--------------->
    <script src="js/mobiscroll_002.js" type="text/javascript"></script>
    <script src="js/mobiscroll_004.js" type="text/javascript"></script>
    <link href="js/mobiscroll_002.css" rel="stylesheet" type="text/css">
    <link href="js/mobiscroll.css" rel="stylesheet" type="text/css">
    <script src="js/mobiscroll.js" type="text/javascript"></script>
    <script src="js/mobiscroll_003.js" type="text/javascript"></script>
    <script src="js/mobiscroll_005.js" type="text/javascript"></script>
    <link href="js/mobiscroll_003.css" rel="stylesheet" type="text/css">
</head>
<body>
<div id="header">
    <div class="header-left" onclick="return_before()"><span>ASQ</span></div>
    <div class="header-center"></div>
    <div class="header-right"></div>
</div>
<div id="container">
    <div class="assessment_information">
        <p class="gray_s"></p>
        <div class="information_div flex_cont">
            <label>宝宝姓名：</label>
            <p class="flex_item"><input type="text" id="baby_name" placeholder="宝宝姓名"/></p>
        </div>
        <div class="information_div flex_cont">
            <label>宝宝性别：</label>
            <ul id="baby_sex" class="flex_item choose_ul">
                <li class="c_selected" id="boy"><span class="select_option" data-sex="1"></span><span class="choose_item">男</span></li>
                <li id="girl"><span class="select_option" data-sex="0"></span><span class="choose_item">女</span></li>
            </ul>
        </div>
        <div class="information_div flex_cont">
            <label>出生日期：</label>
            <p class="flex_item"><input type="text" class="baby_birthday" placeholder="请选择" readonly="" name="time" id="appDate" class=""></p>
            <i class="s_icon n_icon"></i>
        </div>
        <div class="information_div flex_cont">
            <label>宝宝孕周：</label>
            <p class="flex_item" id="gestational_date"><input type="number" id="gestational_week"><span>周</span><input type="number" id="gestational_day"/><span>天</span></p>
        </div>
        <div class="information_div flex_cont">
            <label>出生情况：</label>
            <p id="birth_situation" class="flex_item">请选择</p>
            <i class="s_icon n_icon"></i>
        </div>
        <div id="next_button">
            <p class="login_button">下一步</p>
        </div>
    </div>
</div>
<div id="layer_mark">
    <div id="placeholder"></div>
    <div id="layer_content">
        <div id="layer_title">根据实际情况选择
            <div id="layer_submit">确认</div>
        </div>
        <ul>
            <li class="flex_cont"><p class="flex_item">足月</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">早产</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">顺产</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">剖腹产</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">产前助产</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">吸引器助产</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">双胎</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item">多胎</p><i class="s_icon c_icon"></i></li>
            <li class="flex_cont"><p class="flex_item" id="other_abnormal">其他异常情况</p><i class="s_icon c_icon"></i></li>
        </ul>
        <div id="other_information">
            <textarea placeholder="请输入您的情况"></textarea>
        </div>
        <!--<div id="layer_submit">-->
            <!--<p class="login_button">确认</p>-->
        <!--</div>-->
    </div>
</div>
<script src="js/main.js"></script>
<script src="js/plugins.js"></script>
</body>
</html>
