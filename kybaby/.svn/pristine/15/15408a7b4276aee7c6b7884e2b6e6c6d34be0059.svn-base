.row{
    width: 100%;
    background-color: white;
}
.font1{
    line-height: 50px;
    margin-left: 5%;
    display: inline-block;
    width: 25%;
    font-size: 0.875em;
}
.font{
    line-height: 35px;
    margin-left: 5%;
    color: darkgrey;
    font-size: 0.875em;
}
ul{
    list-style: none;
    width: 100%;
    overflow: hidden;
    background-color: white;
    padding-bottom: 5px;
}
li{
    float: left;
    margin-left:15px;
    text-align: center;
}
li span{

    border: 1px solid #ff813d;
    color:#ff813d ;
    font-size: 0.875em;
    border-radius: 5px;
    padding: 5px;
    display: inline-block;
}
li span.on{
    color:#fff;
    background:#ff813d;
}
#doctor_specialty{
    width:70%;
    white-space:nowrap;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    overflow: hidden;
}
#doctor_data {
    margin: 0px 0 0 8px;
}
#doctor_head{
    margin-top: 12px;
}
#doctor_information {
     background: #fff;
}
p.gray_1 {
    height: 6px;
    background: #F4F4F4;
}
#doctor_head {
    width: 65px;
    height: 65px;
    border-radius: 65px;
}
.row>img{
    width: 17px;
}
.row {
    height: 35px;
    line-height: 35px;
}
#text_evaluation{
    padding: 12px;
}
