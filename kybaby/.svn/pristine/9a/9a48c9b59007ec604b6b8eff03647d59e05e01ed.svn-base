#container{
    line-height: 55px;
    font-size: 14px;
}
#service_name{
    background: #F8F8F8;
    box-shadow: 0px 5px 5px 0px #eee;
    margin-bottom: 10px;
    padding: 0 18px;
    line-height: 40px;
}
#service_time{
    background: #F8F8F8;
    box-shadow: 0px 5px 5px 0px #eee;
}
.padding,#pay_type>div,#pay_subtotal{
    padding: 0 18px;
}
#pay_type>p{
    font-size: 15px;
    color: #FF813D;
}
#pay_subtotal{
    margin-top: 30px;
    text-align: right;
}
#pay_subtotal>span{
    color: #FF8644;
    font-size: 17px;
}
#pay_type img{
    width: 27px;
    margin-right: 5px;
}
#pay_type>div{
    overflow: hidden;
}
.options{
    display: inline-block;
    float: right;
    width: 50px;
    height: 50px;
    background: url(../images/images_family_doctor/select.png)no-repeat right center;
    background-size: 18px;
}
.selected .options{
    background: url(../images/images_family_doctor/true.png)no-repeat right center;
    background-size: 18px;
}
.left{
    display: inline-block;
    float: left;
}
.right{
    display: inline-block;
    float: right;
}
.padding{
    overflow: hidden;
}
.accountBalance{
    padding: 0 18px;
    overflow: hidden;
    line-height: 55px;
}
.accountBalance-left{
    float: left;
    width: 230px;
}
.accountBalance-right{
    width: 50px;
    float: right;
    height: 50px;
}
.accountBalance-right>p{
    width: 50px;
    height: 50px;
    background: url("../images/images_family_doctor/select.png") no-repeat right center;
    background-size: 18px;
}
.accountBalance-right>p.choose{
    background: url("../images/images_family_doctor/true.png") no-repeat right center;
    background-size: 18px;
}
.money{
    padding: 0 18px;
    overflow: hidden;
    line-height: 55px;
}
p.small_p{
    color: #505050;
    font-size: 10px;
    margin-top: -6px;
    line-height: 20px;
    padding-bottom: 6px;
    color: #b0b0b0;
}