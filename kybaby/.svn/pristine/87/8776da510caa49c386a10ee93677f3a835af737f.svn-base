#container{
    font-size: 15px;
}

.personal_list > div {
    width: 100%;
    margin: 5px 0;
    padding: 0px 15px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    overflow: hidden;
}

.personal_list .personal_left, .personal_list .personal_left1 {
    float: left;
    height: 50px;
    line-height: 50px;
    width: 90%;
}

.personal_list .personal_right {
    float: right;
    font-size: 2em;
    color: #c8c8c8;
    width: 10%;
    height: 50px;
    line-height: 44px;
    text-align: right;
}

.personal_list .personal_left1 {
    line-height: 25px;
    font-size: 13px;
}
.personal_list .personal_left img{
    margin-right: 5px;
    width: 26px;
}
.personal_list .personal_right img{
    height: 11px;
    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5s;
    -ms-transition: all 0.5s;
    -o-transition: all 0.5s;
    transition: all 0.5s;
}
.personal_left1 > span {
    border: 1px solid #ff813d;
    color: #ff813d;
    padding: 1px 2px;
    border-radius: 3px;
}
.content{
    font-size: 14px;
    line-height: 21px;
    text-indent: 24px;
    display: none;
}
.zhuan{
    transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
}

