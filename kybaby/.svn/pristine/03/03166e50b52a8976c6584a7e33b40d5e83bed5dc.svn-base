.ul_box{
    margin: 11px 0;
    width: 100%;
    height: 17px;
    background-color: white;
    color: #000000;
}
.ul_box ul{
    list-style: none;
    width: 100%;
    /*height: 100%;*/
}
.ul_box ul#select_cli1 li{
    width: 24.6%;
    float: left;
    text-align: center;
    font-size: 0.875em;
    color: #7D7D7D;
    line-height: 15px;
}
.ul_box ul#select_cli li{
    width: 24.6%;
    float: left;
    text-align: center;
    font-size: 0.875em;
    color: #7D7D7D;
    line-height: 15px;
}
.ul_box ul#select_cli li.blue{
    color:#FF9966;
}
.ul_box ul#select_cli1 li.blue{
    color:#FF9966;
}

.org_erbao,.org_jimian{

}
.org_erbaolist,.org_jimianlist{
    overflow: hidden;
    /*border-top: 5px solid #ccc;*/
    padding: 5px 15px 8px;
    border-bottom: 1px solid #e8e8e8;
}
.head{
    line-height: 32px;
    /*border-bottom: 1px solid #dddddd;*/
    overflow: hidden;
    color: #909090;
    font-size: 12px;
    background: #ffffff;
}
.left{
    width: 60%;
    float: left;
}
.right{
    float: right;
    width: 40%;
    text-align: right;
}
.content{
    overflow: hidden;
    color: #909090;
    font-size: 12px;
    line-height: 22px;
    background: #ffffff;
}
.con_left{
    width: 24%;
    text-align: center;
    float: left;
}
.con_left>img{
    width: 70px;
    height: 70px;
    margin-top: 9px;
}
.con_right{
    width: 72%;
    float: left;
    margin-left: 4%;
}
.orgname{
    color: #505050;
    font-size: 15px;
    margin-bottom: 5px;
}