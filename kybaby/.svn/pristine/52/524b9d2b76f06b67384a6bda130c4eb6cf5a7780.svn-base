#payMethodCover{
    position: fixed;
    width: 100%;
    left: 0;
    top: 0;
    background: rgba(0,0,0,.5);
    z-index: 10000;
    height: 100%;
}
#payMethod{
    position: absolute;
    width: 80%;
    left: 10%;
    top: 50%;
    margin-top: -106px;
    background: #ffffff;
    text-align: center;
    line-height: 40px;
    border-radius: 8px;
}
#chooseWay{
    padding: 10px 0;
}
#chooseWay>div{
    overflow:hidden;
}
#chooseWay>div>p{
    width: 60%;
    float: left;
}
#chooseWay>div>div{
    width: 40%;
    float: left;
}
#chooseWay>div>div>img{
    width: 20px;
}

#isSure{
    overflow: hidden;
    padding: 15px;
}
.cancel{
    width: 35%;
    float: left;
    margin-left: 10%;
    background: #d5d5d5;
    border-radius: 8px;
}
.sure{
    width: 35%;
    float: left;
    margin-left: 10%;
    background: #ff813d;
    color: #ffffff;
    border-radius: 8px;
}