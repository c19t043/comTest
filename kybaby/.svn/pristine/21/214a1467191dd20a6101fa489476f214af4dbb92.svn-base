.ul_box{
    margin: 11px 0;
    width: 100%;
    height: 17px;
    background-color: white;
    color: #000000;
}
.ul_box ul{
    list-style: none;
    width: 100%;
    overflow: hidden;
}
.ul_box ul#select_cli1 li{
    width: 24.6%;
    float: left;
    text-align: center;
    font-size: 0.875em;
    color: #7D7D7D;
    line-height: 15px;
}
.ul_box ul#select_cli li{
    width: 19.6%;
    float: left;
    text-align: center;
    font-size: 0.875em;
    color: #7D7D7D;
    line-height: 15px;
}
.ul_box ul#select_cli li.blue{
    color:#FF9966;
}
.ul_box ul#select_cli1 li.blue{
    color:#FF9966;
}

/*内容*/
.info_box{
    width: 100%;
    /*height: 200px;*/
    background-color: white;
}
.top_sj{
    display: inline-block;
    width: 50%;
    height: 50px;
    line-height: 50px;
    font-size: 0.875em;
    text-indent: 1em;
    float:left;
}
.top_zt{
    margin-right: 10px;
    float: right;
    display: inline-block;
    /*width: 25%;*/
    text-align: center;
    height: 50px;
    line-height: 50px;
    font-size: 0.875em;
    color: #fe4444;
}
.top_je{
    display: inline-block;
    width: 100%;
    line-height: 50px;
    font-size: 0.875em;
    text-indent: 1em;
}
.info{
    width: 100%;
    height: 100px;
    display: inline-block;
}
.info_box .img{
    float: left;
    width: 80px;
    height: 80px;
    /*background-color: #000000;*/
    margin-left: 1em;
    margin-top: 10px;
    margin-bottom: 10px;
    text-align: center;
}
.text_box{
    margin: 10px 10px 10px 110px;
    height: 80px;
}
.font1{
    line-height: 25px;
    font-size: 0.875em;
}
.font2{
    color: #fe4444;
    font-size: 1.25em;
}
.button{
    color: #ff831d;
    border: 1px solid #ff831d;
    text-align: center;
     padding:0 5px 0 5px ;
    height: 30px;
    line-height: 30px;
    display: inline-block;
    float: right;
    border-radius: 5px;
    margin-right: 10px;
    text-indent: 0px;
    margin-top: 9px;
}
