.ul_box{
    width: 100%;
    height: 50px;
    background-color: white;
    color: #000000;
}
.ul_box ul{
    list-style: none;
    width: 100%;
}
.ul_box ul li{
    float: left;
    width: 20%;
    height: 48px;
    line-height: 48px;
    text-align: center;
    font-size: 0.875em;
}
.blue{
    color:  #ff813d;
    border-bottom: 3px solid  #ff813d;
}


