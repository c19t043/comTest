.personal_list .personal_right input{
    border: 1px solid #CCCCCC;
    padding-left: 10px;
    width: 86%;
    -webkit-box-shadow: 0px -1px 1px 0px #ccc;
    -moz-box-shadow: 0px -1px 1px 0px #ccc;
    box-shadow: 0px -1px 1px 0px #ccc;
    height: 30px;
    border-radius: 6px;
    color: #505050;
}

#cover{
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.3);
    z-index: 2000;
    display: none;
    font-size: 14px;
}
#cover>div{
    width: 246px;
    position: absolute;
    left: 50%;
    margin-left: -131px;
    top: 98px;
    padding: 8px;
    max-height: 305px;
    border: 2px solid #f7f7f7;
    background: #ff813d;
    border-radius: 6px;
}
#cover>div>p{
    line-height: 26px;
    text-align: center;
    color: #ffffff;
}
#cover>div>div{
    background: #ffffff;
    border-radius: 6px;
    max-height: 279px;
    overflow-y: scroll;
    text-align: center;
    width: 246px;
}

#cover table{
    background: #ffffff;
    border-radius: 6px;
    width: 246px;
}
#cover table tr{
    height: 54px;
    border-bottom: 1px solid #cccccc;
}
#cover>p{
    position: absolute;
    width: 22px;
    height: 22px;
    background: #ffffff;
    right: 70px;
    top: 90px;
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    transform: rotate(45deg);
}