
body {
    background: #ffffff;
    color: #505050;
}

.gray_ss {
    height: 4px;
    width: 100%;
    background: #F7F7F7;
}

.gray_s {
    height: 10px;
    width: 100%;
    background: #F7F7F7;
}

.personal_list > div {
    width: 100%;
    padding: 0px 15px 0px 30px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    overflow: hidden;
}

.personal_list .personal_left{
    float: left;
    height: 50px;
    line-height: 50px;
    width: 30%;
}

.personal_list .personal_right {
    float: right;
    width: 68%;
    height: 50px;
    line-height: 50px;
}
.personal_list .personal_right div{
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    border: 1px solid #CCCCCC;
    width: 86%;
    -webkit-box-shadow: 0px -1px 1px 0px #ccc;
    -moz-box-shadow: 0px -1px 1px 0px #ccc;
    box-shadow: 0px -1px 1px 0px #ccc;
    height: 30px;
    line-height: 30px;
    border-radius: 6px;
    padding-left: 10px;
    margin-top: 10px;
    color: #505050;
    overflow: hidden;
    position: relative;
}
.personal_list .personal_right .selectmenu{
    position: absolute;
    right: 0px;
    top:0px;
    background: #ffffff;
    width:32px;
    text-align: center;
}
.personal_list .personal_right .selectmenu img{
    width: 20px;
}
