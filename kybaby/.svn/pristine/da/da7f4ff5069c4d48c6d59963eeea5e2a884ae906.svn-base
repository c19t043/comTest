*{
    margin: 0;
    padding: 0;
}
html,body{
    height: 100%;
}
#header{
    position: relative;
    z-index: 10;
}
.con{
    width: 100%;
    height: 100px;
    background-color: #f7f7f7;
}
.box{
    width: 320px;
    margin: 0 auto;
    height: 140px;
    overflow: auto;
    white-space: nowrap;
    background-color: white;
    z-index: 8;
    position: relative;
}
.block{
    display: inline-block;
    border-right: 1px solid #000000;
    height: 60px;
    width: 159px;
    position: relative;
}
.hm{
    display: inline-block;
    border-right: 1px solid darkgrey;
    width: 15px;
    height: 50px;
    position: relative;
    bottom: 10px;
}
.box::-webkit-scrollbar {
    display: none;
}
.zhizhen{
    position: absolute;
    width:50%;
    height: 50px;
    border-right:2px solid  #FF813D;
    top: 150px;
    z-index: 19;
    left: -1px;
}
.kd{
    position: absolute;
    font-size: 1.5em;
    top: 60px;
    right: -8px;
}
.kds{
    position: absolute;
    font-size: 1.5em;
    top: 60px;
    right: -15px;
}
.kdss{
    position: absolute;
    top: 28px;
    right: 70px;
}
.sj{
    position: absolute;
    top: 302px;
    left: 49%;
    width: 0;
    height: 0;
    border-top: 10px solid transparent;
    border-right: 20px solid #FF813D;
    border-bottom: 10px solid transparent;
}
.wenben{
    position: relative;
    z-index: 8;
    width: 100%;
    height: 140px;
    text-align: center;
    color: #FF813D;
    background:#f7f7f7;
}
.up{
    position: absolute;
    top: 148px;
    width: 100%;
    z-index: 8;
}
.imgbox{
    width: 330px;
    margin: 0 auto;
}
.up img{
    width: 330px;
}
.down{
    position: absolute;
    top: 310px;
    width: 100%;
    z-index: 8;

}
.down img{
    width: 100%;
}
#num{
    display: inline-block;
    font-size: 3em;
    line-height: 50px;
    margin-top: 20px;
    position: relative;
    z-index: 10;
}
#nums{
    font-size: 1.5em;
    line-height: 30px;
    position: relative;
    z-index: 10;
}


footer{
    position: fixed;
    bottom: 0px;
    width: 100%;
    height: 50px;
    line-height: 50px;
    background-color: white;
    color: darkgrey;
    text-align: center;
    z-index: 10;
}