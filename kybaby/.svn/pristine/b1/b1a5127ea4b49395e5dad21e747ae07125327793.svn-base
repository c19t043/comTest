/*积分 余额*/
.balance_top {
	width: 100px;
	height: 100px;
	margin: 20px auto;
	background: #ff813d;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	color: #fff;
	text-align: center;
}
.balance_top p:first-child {
	font-size: 1.375em;
	padding-top: 25px;
}
.balance_top p:last-child {
	font-size: 0.875em;
	line-height: 20px;
}
.balance_detail {
	width: 90%;
	margin: 0 auto;
	clear: both;
	overflow: hidden;
}
.balance_detail_tit {
	width: 96%;
	padding: 0 2%;
	font-size: 0.75em;
	color: #ff813d;
	border-bottom: 1px solid #c8c8c8;
	line-height: 24px;
}
.balance_list {
	width: 96%;
	padding: 10px 2%;
	border-bottom: 1px solid #c8c8c8;
	clear: both;
	overflow: hidden;
}
.balance_list .date {
	width: 100%;
	font-size: 0.75em;
	line-height: 20px;
	margin-bottom: 10px;
}
.balance_list .project {
	width: 50%;
	float: left;
	/*color: #c8c8c8;*/
	font-size: 0.875em;
	line-height: 20px;
}
.balance_list .sum {
	width: 50%;
	color: #558ed5;
	float: right;
	text-align: right;
	font-size: 0.875em;
	line-height: 20px;
}