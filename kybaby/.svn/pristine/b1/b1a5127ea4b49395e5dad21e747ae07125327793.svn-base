@charset "utf-8";

/*个人中心*/
.personal_box {
	width: 90%;
	padding: 10px 5%;
	background: #fff;
	clear: both;
	overflow: hidden;
}
.personal_left {
	width: 70px;
	height: 70px;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	float: left;
}
.personal_left img {
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	width: 70px;
	height: 70px;
}
.personal_right {
	margin-left: 90px;
	background: url("../images/next.png") no-repeat right center;
	background-size: 9px;
}
.personal_right p {
	line-height: 1.5em;
}
/*.personal_right .username {*/
	/*font-size: 1.125em;*/
/*}*/
.myorder {
	width: 33%;
	float: left;
	text-align: center;
	background-size: 9px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
	font-size: 15px;
}
#mypersonal_box{
	width: 100%;
	background: #F8F8F8;
	padding: 15px 0;
	box-shadow: 0px 5px 5px 0px #eee;
}
#mypersonal_box span{
	display: inline-block;
	border-right: 1px solid #6E6E6E;
	width: 100%;
	line-height: 16px;
}
.myorg {
    width: 34%;
    float: left;
    text-align: center;
    /*border-right: 1px solid #dcdcdc;*/
    /*background: url("../images/next.png") no-repeat 90% center;*/
    background-size: 9px;
    /*line-height: 50px;*/
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.mydoctor {
    width: 33%;
    float: left;
	text-align: center;
	/*background: url("../images/next.png") no-repeat right center;*/
	background-size: 9px;
	/*line-height: 50px;*/
}
.personal_list {
	background: #fff;
	font-size: 15px;
}
.personal_com {
	width: 80%;
	padding-left: 10%;
	margin: 0 auto;
	line-height: 50px;
}
.tool{
	margin-top: 20px;
    padding: 6px 5%;
    overflow: hidden;
}
.tool>div{
    width: 33.3%;
    float: left;
    text-align: center;
}
.tool>p{
