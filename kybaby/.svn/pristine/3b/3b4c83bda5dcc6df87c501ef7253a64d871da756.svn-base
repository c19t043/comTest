<!doctype html>
<html class="no-js" lang="zh-cn">
<head>
	<title>首页</title>
	<meta charset="utf-8">
	<meta http-equiv="x-ua-compatible" content="ie=edge">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1"/>
	<link rel="shortcut icon" href="favicon.ico" type="image/x-icon"/>
	<link rel="stylesheet" href="css/normalize.css">
	<link rel="stylesheet" href="css/main.css">
	<link rel="stylesheet" href="css/home.css">
	<script src="js/modernizr-2.8.3.min.js"></script>
	<script src="js/jquery-1.11.3.min.js"></script>
</head>
<body>

<div id="header">
    <div class="header-left" onclick="return_before()"><span>首页</span></div>
	<div class="header-center"></div>
	<div class="header-right"></div>
</div>
<!--header-->

<div id="container">
	<div id="banner">
		<div class="sliderContainer fullWidth clearfix">
			<div id="full-width-slider" class="royalSlider heroSlider rsMinW">
				<div class="rsContent"><img class="rsImg" src="images/1.jpg" /></div>
				<div class="rsContent"><img class="rsImg" src="images/2.jpg" /></div>
				<div class="rsContent"><img class="rsImg" src="images/3.jpg" /></div>
			</div>
		</div>
	</div>
	<p class="gray_1"></p>

	<div class="topservice_box">
		<div class="topservice_left">
			<p>快速咨询</p>

			<p>已为您匹配最好的医生</p>
		</div>
		<div class="topservice_right">
			<p>名医上门</p>

			<p>名医为你提供优质服务</p>
		</div>

	</div>
	<p class="gray_1"></p>
	<!--未注册-->
	<div class="content01">
		<!--特色定制服务产品-->
		<div class="service_porduct">
			<p class="tit_nor">特色定制服务产品</p>

			<div class="product_box">
				<p class="prev_box"></p>

				<p class="next_box"></p>

				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
			</div>
		</div>
		<p class="gray_1"></p>

		<div class="plan_box">
			<p class="tit_nor">健康计划管理方法</p>

			<p class="plan_img"></p>

		</div>


	</div>

	<!--已注册-->
	<div class="content02">
		<!--量身定制服务产品-->
		<div class="service_porduct">
			<p class="tit_nor">量身定制服务产品</p>

			<div class="product_box">
				<p class="prev_box"></p>

				<p class="next_box"></p>

				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
				<div class="product_list"></div>
			</div>
		</div>
		<p class="gray_1"></p>

		<div class="plan_box">
			<p class="tit_nor">量身定制健康计划</p>

			<div class="health_box">
				<div class="health_left"></div>
				<div class="health_right">
					<p>李医生</p>

					<p class="doctor_zhicheng">职称：<span>主治医师</span></p>

					<div class="health_product">
						<p>称重计划</p>

						<div class="health_tag">
							<p class="select"><span>多运动</span><span class="foot"></span></p>

							<p><span>多喝水</span><span class="foot"></span></p>

							<p><span>吃鸡蛋</span><span class="foot"></span></p>
						</div>
					</div>
				</div>
			</div>

		</div>


	</div>
	<p class="gray_1"></p>


</div>
<!--container-->

<div id="footer">
	<ul>
		<a href="javascript:void(0)">
			<li class="select"><p><img src="images/footer_icon01_on.png" width="23"></p>

				<p>首页</p></li>
		</a>
		<a href="javascript:void(0)">
			<li><p><img src="images/footer_icon02_off.png" width="23"></p>

				<p>服务</p></li>
		</a>
		<a href="javascript:void(0)">
			<li><p><img src="images/footer_icon03_off.png" width="23"></p>

				<p>助手</p></li>
		</a>
		<a href="javascript:void(0)">
			<li><p><img src="images/footer_icon04_off.png" width="23"></p>

				<p>个人</p></li>
		</a>
	</ul>
</div>
<!--footer-->

<!--banner js-->
<script>
	$(document).ready(function () {
		$("#banner img").css({
			"display": "block",
			"margin-top": "0",
			"margin-left": "0",
			"height": "0",
		})
	})
</script>
<script type="text/javascript" src="js/jquery.royalslider.min.js"></script>
<link href="css/royalslider_home.css" rel="stylesheet" type="text/css"/>
<script>
	jQuery(document).ready(function ($) {
		$('#full-width-slider').royalSlider({
			arrowsNav: true,
			loop: true,
			//loopRewind: true,
			fadeinLoadedSlide: true,
			slidesSpacing: 0,
			keyboardNavEnabled: true,
			controlsInside: true,
			imageScaleMode: 'fill',
			arrowsNavAutoHide: false,
			autoScaleSlider: true,
			autoScaleSliderWidth: 800,
			autoScaleSliderHeight: 400,
			controlNavigation: 'bullets',
			thumbsFitInViewport: false,
			navigateByClick: false, //Navigates forward by clicking on slide.
			startSlideId: 0,
			autoPlay: {
				enabled: true, //autoPlay or not
				stopAtAction: false, //Stop autoPlay at first user action
				pauseOnHover: false, //Pause autoPlay on hover
				delay: 3000 //Delay between items in ms.
			},
			transitionType: 'move',
			globalCaption: true,
			deeplinking: {
				enabled: true,
				change: false
			},
			imgWidth: 800,
			imgHeight: 400
		});
	});
</script>
<script>
	$(document).ready(function () {
		var x = $("#banner img").height();
		$("#banner img").css({
			"height": x,
			"width": "100%",
		});
		$("#full-width-slider ").css("height", x);
		$(".rsOverflow ").css("height", x);
	})
</script>
<!--banner js end-->


<script src="js/main.js"></script>
<script src="js/plugins.js"></script>

</body>
</html>
