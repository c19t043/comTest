body{
    background: #fff;
}
.topic_title{
    padding: 15px;
    line-height: 25px;
    word-wrap:break-word;
}
.topic_div ul{
    padding-top: 8px;
}
.according_content>.topic_div>ul>li:last-child>.options_li{
    border-bottom: 0px;
}
.options_li{
    border-bottom: 1px dashed #e8e8e8;
}
/*.question_img{*/
    /*padding: 8px 0;*/
/*}*/
.topic_div{
    border: 3px solid rgba(0,0,0,0);
}
@-webkit-keyframes twinkling{
    0%{
        border: 3px solid rgba(1,1,1,0);
    }
    100%{
        border: 3px solid #ff813d;
    }
}
.topic_div li,.checked{
    padding: 0 27px;
    font-size: 13px;
    line-height: 36px;
}
.checked{
    padding-bottom: 8px;
}
.question_img{
    padding: 8px 15px 0;
}
.c_icon{
    background: url("../images/images_family_doctor/select.png")no-repeat right center;
    background-size: 13px;
}
.c_icon.choose{
    background: url("../images/images_family_doctor/true.png")no-repeat right center;
    background-size: 13px;
}
#topic_details>div{
    display: none;
}
#topic_details>div.according_content{
    display: block;
}
.other_information {
    padding: 4px 18px 0 0px;
    display: none;
}
.other_information>textarea {
    padding: 3px 9px;
    width: 100%;
    height: 62px;
    background: #f4f4f4;
    border: none;
    outline: none;
    font-size: 12px;
    resize: none;
}
#foot_button{
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 5px 18px;
    background: #fff;
    overflow: hidden;
}
#foot_button>#prev{
    float: left;
    display: none;
}
#foot_button>#next{
    float: right;
}
.resultTag ul {
    height: 34px;
    width: 100%;
    overflow-x: auto;
    overflow-y: hidden;
    white-space: nowrap;
}
.resultTag .tag_li.tagOld {
    background: #f6c2a6;
    color: #505050;
}
.resultTag .tag_li {
    background: #f8e7de;
    height: 34px;
    line-height: 34px;
    text-align: center;
    display: inline-block;
    font-size: 14px;
    color: #909090;
    position: relative;
    padding: 0 20px;
    min-width: 60px;
    /*margin-left: -3px;*/
}
.resultTag .tag_li.tagOld:first-of-type:before {
    border-left: 10px solid #f6c2a6;
}
.resultTag .tag_li:last-of-type:after {
    border-left: 10px solid #f6c2a6;
    border-bottom: 0px solid rgba(0,0,0,0);
    border-top: 0px solid rgba(0,0,0,0);
}
.resultTag .tag_li.tagOld:before {
    background: #f6c2a6;
}
.resultTag .tag_li:before {
    display: inline-block;
    background: #f8e7de;
    content: '';
    border-left: 10px solid white;
    border-bottom: 17px solid rgba(0,0,0,0);
    border-top: 17px solid rgba(0,0,0,0);
    position: absolute;
    top: 0px;
    left: 0px;
}
.resultTag .tag_li.tagOld:after {
    border-left: 10px solid #f6c2a6;
}
.resultTag .tag_li:after {
    display: inline-block;
    background: #fff;
    content: '';
    border-left: 10px solid #f8e7de;
    border-bottom: 17px solid rgba(0,0,0,0);
    border-top: 17px solid rgba(0,0,0,0);
    position: absolute;
    top: 0px;
    right: 0px;
}
.b_icon,.e_icon{
    background: url("../images/images_icon/uncheck.png")no-repeat right center;
    background-size: 13px;
}
.b_icon.b_selected,.e_icon.e_selected{
    background: url("../images/images_icon/selected.png")no-repeat right center;
    background-size: 13px;
}
.submit{
    width: 45px;
    text-align: center;
}