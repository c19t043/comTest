<!doctype html>
<html class="no-js" lang="zh-cn">
<head>
    <title>订单详情</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1"/>
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" href="css/order_details.css">
    <script src="js/modernizr-2.8.3.min.js"></script>
    <script src="js/jquery-1.11.3.min.js"></script>
    <script src="js/config.js"></script>
	<script src="js/MD5.js"></script>
    <script src="js/order_details.js"></script>
    <script src="js/main.js"></script>
</head>
<body>

<div id="header">
    <div class="header-left" onclick="return_before()"><span>订单详情</span></div>
    <div class="header-center"></div>
    <div class="header-right"></div>
</div>
<!--header-->

<div id="container">
    <p class="gray_2"></p>
    <div class="row">
        <span class="font1">日期：</span><span id="order_time" class="font"></span><br/>
        <span class="font1">订单号：</span><span id="order_num" class="font"></span><br/>
        <span class="font1">订单状态：</span><span id="order_status" class="font"></span>
        <span id="order_status_plus" class="font2"></span>
   </div>
    <p class="gray_1"></p>
    <div class="row">
        <span id="order_userName" class="font3"></span><span id="order_phone" class="font4"></span><br/>
        <span id="order_address_1" class="font1 grey"></span><br/>
        <span id="order_address_2" class="font1 grey"></span>
    </div>
    <p class="gray_1"></p>
    <div id="order_object" class="row">
        <div class="tx">
        	<img id="order_img" />
        </div>
        <span id="order_productName" class="font5"></span><br/>
        <span id="order_doctor" class="font5"></span><br/>
        <span id="order_level" class="font5"></span><br/>

    </div>
    <p class="gray_1"></p>
    <div class="row">
        <span class="font1">总金额</span>
        <span id="order_moneyAll" class="red"></span>
    </div>
    <div class="row">
        <span class="greyl">优惠券</span>
        <span id="order_yhq" class="greyr"></span>
    </div>
    <div class="row">
        <span class="greyl">积分抵现</span>
        <span id="order_jf" class="greyr"></span>
    </div>
    <div class="row">
        <span class="greyl">余额</span>
        <span id="order_ye" class="greyr"></span>
    </div>
    <div class="row">
        <span id="order_payWay" class="greyl"></span>
        <span id="order_otherWayMoney" class="greyr"></span>
    </div>
    <p class="gray_2"></p>
    <div class="row">
        <span id="order_lastMoney" class="red"></span>
        <span class="font1" style="float: right">实际金额:</span>

    </div>

</div>







<div style="display:none;width: 80%; padding: 20px 0px; position: fixed; top: 25%; left: 10%; background: rgb(102, 102, 102);" id="zPayMethod">
	<p style="font-size:18px; color:#FFF; text-align:center; padding:0 0 20px 0;">请选择支付方式</p>
	<p class="selcli" style="color:#00FF44;padding:0 0 10px 0; width:130px; margin:0 auto; text-align:left;"><label style="color:#00FF44;background:rgb(102, 102, 102);opacity:0.9;padding:0 20px 0 0;font-size:16px;" for="wx_way">微信支付:</label><input checked="checked" class="checkbo" mark="微信" id="wx_way" type="radio" name="zhifu" value="weixin" style="position:relative;top:5px;float:right; line-height:21px;"></p>
	<p class="selcli" style="display: none; color:#00FF44;padding:0 0 10px 0; width:130px; margin:0 auto; text-align:left;"><label style="color:#00FF44;background:rgb(102, 102, 102);opacity:0.9;padding:0 20px 0 0;font-size:16px;" for="zfb_way">支付宝支付:</label><input class="checkbo" mark="支付宝" id="zfb_way" type="radio" name="zhifu" value="zhifubao" style="position:relative;top:5px;float:right; line-height:21px;"></p>
	<p class="selcli" style="display: none; color:#00FF44;padding:0 0 10px 0; width:130px; margin:0 auto; text-align:left;"><label style="color:#00FF44;background:rgb(102, 102, 102);opacity:0.9;padding:0 20px 0 0;font-size:16px;" for='yl_way'>银联支付:</label><input class="checkbo" mark="银联" id='yl_way' type="radio" name="zhifu" value="yinlian" style="position:relative;top:5px;float:right; line-height:21px;"></p>
	<p style="text-align:center; margin-top: 10px;">
		<input type="button" value="取消" onclick="quxiao()" style="width: 40%; height: 40px; font-size: 1em; line-height: 1em; -webkit-tap-highlight-color: rgba(0,0,0,0); -webkit-appearance: none; border: none; border-radius: 5px; background-color: #eee; color: #666;" id="btnCancel">
		<input type="button" value="确认" onclick="goPay(this);" style="width:40%; margin:0 0 0 20px; height: 40px; font-size: 1em; line-height: 1em; -webkit-tap-highlight-color: rgba(0,0,0,0); -webkit-appearance: none; border: none; border-radius: 5px; background-color: #eee; color: #666;" id="btnOrderPay">
	</p>
</div>
<script src="js/main.js"></script>
<script src="js/plugins.js"></script>

</body>
</html>
