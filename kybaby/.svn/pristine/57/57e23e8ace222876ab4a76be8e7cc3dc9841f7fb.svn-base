<!doctype html>
<html class="no-js" lang="zh-cn">
<head>
    <title>评价</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1"/>
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/main.css">
    <link rel="stylesheet" href="css/consultation.css">
    <link rel="stylesheet" href="css/consult.css">
    <link rel="stylesheet" href="css/consult_evaluation.css">
    <script src="js/modernizr-2.8.3.min.js"></script>
    <script src="js/jquery-1.11.3.min.js"></script>
    <script src="js/config.js"></script>
    <script src="js/consult_evaluation.js"></script>
    <script src="js/main.js"></script>
</head>
<body>

<div id="header">
    <div class="header-left" onclick="return_before()"><span>评价</span></div>
    <div class="header-center"></div>
    <!--<div class="header-right"><div id="submit" onclick="submit()">提交</div></div>-->
    <div class="header-right"></div>
</div>
<!--header-->

<div id="container">
    <div id="doctor_information">

    </div>
    <p class="gray_1"></p>
    <p id="evaluation_title" style="margin-top: 117px"><span class="divider"></span><span class="title_text">评价您的医生</span><span class="divider"></span></p>
    <div id="star_1_plus" class="row">
        <span class="font1">服务态度</span>
        <img mark="1" class="star_1" src="images/icon_star.png">
        <img mark="2" class="star_1" src="images/icon_star.png">
        <img mark="3" class="star_1" src="images/icon_star.png">
        <img mark="4" class="star_1" src="images/icon_star.png">
        <img mark="5" class="star_1" src="images/icon_star.png">
    </div>
    <div id="star_2_plus" class="row">
        <span class="font1">责任感</span>
        <img mark="1" class="star_2" src="images/icon_star.png">
        <img mark="2" class="star_2" src="images/icon_star.png">
        <img mark="3" class="star_2" src="images/icon_star.png">
        <img mark="4" class="star_2" src="images/icon_star.png">
        <img mark="5" class="star_2" src="images/icon_star.png">
    </div>
    <div id="star_3_plus" class="row">
        <span class="font1">准时度</span>
        <img mark="1" class="star_3" src="images/icon_star.png">
        <img mark="2" class="star_3" src="images/icon_star.png">
        <img mark="3" class="star_3" src="images/icon_star.png">
        <img mark="4" class="star_3" src="images/icon_star.png">
        <img mark="5" class="star_3" src="images/icon_star.png">
    </div>
    <p class="gray_s"></p>
    <div id="text_evaluation">
        <label>评价医生</label>
        <textarea placeholder="请输入您对医生的评价，方便我们改进，谢谢！"></textarea>
    </div>
    <!--<div class="row">-->
        <!--<span class="font">您还可以对医生进行其他评价</span>-->
    <!--</div>-->
    <!--<ul id="tag">-->

    <!--</ul>-->
    <div class="login_button" id="submit" onclick="submit()">提交</div>
</div>


<script src="js/main.js"></script>
<script src="js/plugins.js"></script>

</body>
</html>
