footer{
    display: inline-block;
    width: 100%;
    max-height: 120px;
    background-color: #fff;
    border-top: 1px solid #E7E7E7;
    position: fixed;
    bottom: 0px;
}
.before img{
    width: 30px;
    margin-top: 10px;
    margin-left: 10px;
}
.after img{
    width: 30px;

}
.liability{
    position: fixed;
    left: 0;
    top: 127px;
    width: 100%;
    background: #ffffff;
}
#text_box{
    padding-top: 97px;
}
.liability>span{
    /*padding: 5px 20px;*/
    background:#f7f7f7;
    border-radius: 6px;
    font-size: 12px;
}
footer>.illness_impression{
    padding: 3px 15px;
    font-size: 14px;
}
footer>.illness_impress{
    padding: 3px 15px;
    font-size: 12px;
}
footer>.illness_impress>span{
    display: inline-block;
    padding: 2px 6px;
    color: #ff831d;
}

footer input{
    padding: 0 3%;
    width: 94%;
    height: 30px;
    margin-top: 10px;
    border: none;
    border-bottom:1px solid #FE8C4E;
    background: #f7f7f7;
}
.before{
    display: inline-block;
    width: 10%;
    height: 50px;
    float: left;
}
.after{
    display: inline-block;
    float: right;
    width: 16%;
    text-align: center;
}
.con{
    display: inline-block;
    width: 67%;
    max-height: 120px;
    margin-left: 1%;
    float: left;
}
.con>#newMsgContent{
    width: 100%;
    margin-top: 10px;
    border: none;
    background: #F7F7f7;
    border-bottom: 1px solid #ff831d;
    min-height: 20px;
    max-height: 80px;
    line-height: 20px;
    _height: 80px;
    margin-left: auto;
    margin-right: auto;
    padding: 3px;
    outline: 0;
    font-size: 12px;
    word-wrap: break-word;
    overflow-x: hidden;
    overflow-y: auto;
    -webkit-user-modify: read-write-plaintext-only;
}

.after span{
    display: inline-block;
    width: 85%;
    height: 30px;
    line-height: 30px;
    color: white;
    background: url(../images/images_family_doctor/erbao_point_light.png)no-repeat center;
    background-size: 100% 100%;
    border-radius: 5px;
    text-align: center;
    margin: 10px 15px 0 0;
    position: relative;
    top: 1px;
}
.tittle{
    width: 100%;
    display: inline-block;
    text-align: center;
    height: 40px;
    line-height: 40px;
    color:#50c1e9 ;
}


/*new*/
.consultop {
    width: 90%;
    padding: 0 5%;
    background: #fff;
    margin: 15px auto;
}
.consultop_time {
    width: 80%;
    text-align: right;
    color: #c8c8c8;
    line-height: 15px;
    font-size: 11px;
}
.consultop_tab {
    width: 80%;
    margin: 0 auto;
    clear: both;
    overflow: hidden;
}
.consultop_tab p {
    padding: 5px;
    margin: 2px 3px;
    float: right;
    color: #969696;
    background: #f7f7f7;
    line-height: 1.5em;
    font-size: 0.875em;
    border-radius: 5px;
}
.consultop_img {
    width: 80%;
    margin: 10px auto;
    clear: both;
    overflow: hidden;
}
.consultop_img img {
    float: right;
}
.user_info {
    clear: both;
    overflow: hidden;
}
.user_txt {
    max-width: 70%;
    margin: 5px 15px 0 0;
    padding: 5px 2%;
    background: #ff813d;
    color: #000;
    float: right;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    font-size: 0.875em;
    line-height: 1.5em;
}
.user_photo {
    width: 15%;
    float: right;
}
.user_photo img {
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    width:40px;
    height:40px;
}


/*新增*/
.consulleft {
    width: 90%;
    padding: 0 5%;
    margin: 15px auto;
    background: #fff;
    clear: both;
    overflow: hidden;
}
.consulleft_photo {
    width: 15%;
    margin-top: 10px;
    float: left;
}
.consulleft_photo img {
    width: 40px;
    height: 40px;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
}
.consulleft_txt {
    margin-left: 18%;
}
.consulleft_txt .time {
    width: 100%;
    color: #c8c8c8;
    line-height: 15px;
    font-size: 10px;
    text-align: left;
}
.consulleft_txt .text {
    max-width: 80%;
    padding: 5px 2%;
    background: #fff;
    color: #000;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    display: inline-block;
    font-size: 0.875em;
    line-height: 1.5em;
    margin-top: 5px;
    border:2px solid #FD8D4E;
    word-break:break-all;
}

.consulright {
    width: 90%;
    padding: 0 5%;
    margin: 15px auto;
    background: #fff;
    clear: both;
    overflow: hidden;
}
.consulright_txt {
    width: 80%;
    float: left;
}
.consulright_txt .time {
    width: 100%;
    color: #c8c8c8;
    line-height: 15px;
    font-size: 11px;
    text-align: right;
}
.consulright_txt .text {
    max-width: 80%;
    padding: 5px 2%;
    background: #ff813d;
    color: #000;
    float: right;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    display: inline-block;
    font-size: 0.875em;
    line-height: 1.5em;
    margin-top: 5px;
    word-break:break-all;
}
.consulright_photo {
    width: 15%;
    margin-top: 10px;
    float: right;
}
.consulright_photo img {
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
 width: 40px;
    height: 40px;
}

.consulright_txt .ysk_yy{
	float:right;
}
.consulleft_txt .ysk_yy{
	float:left;
}

