.row{
    display: inline-block;
    width: 100%;
    height: 50px;
    line-height: 50px;
    background-color: white;
    font-size: 0.875em;
    text-indent: 1em;
}
.font1{
    margin-left: 5px;
}
.font2{
    margin-left: 15px;
    color: #c8c8c8;
}
.font3{
    float: left;
    margin-left: 5em;
    color: #c8c8c8;
    line-height: 50px;
    clear: both;
}
.font4{
    float: right;
    margin-right: 5em;
    color: #c8c8c8;
    line-height: 50px;
    clear: both;
}
.biaoqian {
    float: right;
    margin-right: 5em;
    background-color: #F7F7F7;
    color: #969696;
