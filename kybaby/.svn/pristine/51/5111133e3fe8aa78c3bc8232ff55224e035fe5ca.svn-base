/*新增*/
#doctor_information{
    padding:5px 15px;
    text-align: center;
    background: #f7f7f7;
    overflow: hidden;
    line-height: 30px;
    font-size: 15px;
    position: fixed;
    left: 0;
    top: 45px;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#doctor_head{
    border: 1px solid #eee;
    display: inline-block;
    width: 70px;
    height: 70px;
    border-radius: 70px;
    overflow: hidden;
    float: left;
}
#doctor_head>img{
    width: 100%;
}
#item_name{
    overflow: hidden;
    box-shadow: 0 0 15px 6px #F7F7f7;
}
#item_name>div{
    width: 100%;
    float: left;
    text-align: center;
}
#item_name>div>span{
    margin: 6px 0;
    padding: 4px;
    display: inline-block;
}
