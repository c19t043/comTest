.mypage_personal .myarticle {
  width: 90%;
  padding: 0 5% 10px 5%;
  margin-top: 10px;
  font-size: 0.875em;
  border-bottom: 1px solid #f7f7f7;
  clear: both;
  overflow: hidden;
}

.mypage_personal .myarticle .article_date,
.mypage_personal .myarticle .article_title {
  line-height: 24px;
}

.mypage_personal .myarticle .comment {
  background: url(../images/icon_comment.png) no-repeat left center;
  background-size: 16px;
  width: 30%;
  float: left;
  padding-left: 20px;
  line-height: 30px;
  font-size: 0.75em;
  color: #c8c8c8;
}

.mypage_personal .myarticle .view {
  background: url(../images/icon_view.png) no-repeat left center;
  background-size: 16px;
  width: 30%;
  float: left;
  padding-left: 25px;
  line-height: 30px;
  font-size: 0.75em;
  color: #c8c8c8;
}

.mypage_personal .myarticle .right {
  width: 20%;
  float: right;
  line-height: 30px;
  font-size: 1.5em;
  text-align: center;
  color: #c8c8c8;
}