@charset "utf-8";

.top_box {
	width: 100%;
	height: 50px;
	line-height: 50px;
	color: #969696;
	background: #fff;
}

.top_box ul li {
	float: left;
	width: 30%;
	margin: 0 1%;
	height: 46px;
	text-align: center;
}
.top_box ul li.center {
	margin: 0 3%;
}
.top_box ul li.select {
	border-bottom: 4px solid #ff813d;
	color: #ff813d;
}



.height_box,
.weight_box,
.head_box {
	width: 100%;
	padding: 20px 0;
	min-height: 300px;
	background: #fff;
	display: none;
}

.record_round {
	width: 100px;
	height: 100px;
	margin: 0 auto;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	background: #ff813d;
	text-align: center;
	border: 5px solid #FABD9C;
	color: #fff;
}
.record_round p:first-child {
	font-size: 2em;
	padding-top: 15px;
}
.record_round p:last-child {
	line-height: 16px;
}
.normal_range {
	text-align: center;
	color: #c8c8c8;
	font-size: 0.875em;
	line-height: 2em;
}
.record_note {
	width: 80%;
	margin: 10px auto;
	border: 1px solid #c7c7c7;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	padding: 6px 3%;
	font-size: 0.875em;
}
.tu_box {
	width: 100%;
	padding: 10px 0;
	background: #fff;
}
.tu_tab {
	width: 60%;
	margin: 6px auto 0px auto;
	height: 30px;
	line-height: 30px;
	clear: both;
	overflow: hidden;
	border: 1px solid #ff813d;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	color: #bbbbbb;
	font-size: 0.875em;
	text-align: center;
}
.tu_tab .qushitu {
	float: left;
	width: 50%;
}
.tu_tab .lishijilu {
	float: right;
	width: 50%;
}
.tu_tab .select {
	background: #ff813d;
	color: #fff;
}
.qushi_box,
.lishi_box {
	width: 100%;
	margin: 10px 0;
	/*background: #000;*/
	display: none;
}
.small {
	font-size: 0.75em;
	width: 95%;
	margin: 10px auto;
	text-align: center;
}
.lishi_tit {
	width: 100%;
	clear: both;
	overflow: hidden;
	background: #ff813d;
	color: #fff;
	line-height: 2em;
	font-size: 0.875em;
}
.lishi_tit p {
	width: 25%;
	float: left;
	text-align: center;
}
.lishi_tit p.center {
	margin: 0 5%;
}
.lishi_list {
	width: 100%;
	clear: both;
	overflow: hidden;
	line-height: 2.5em;
	font-size: 0.875em;
	border-bottom: 1px solid #ff813d;
}
.lishi_list p {
	width: 25%;
	float: left;
	text-align: center;
}
.lishi_list p.center {
	margin: 0 5%;
}



