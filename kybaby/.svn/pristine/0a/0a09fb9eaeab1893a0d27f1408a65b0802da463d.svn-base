
* {
    margin: 0px;
    padding: 0px;
}

header {
    overflow: hidden;
    padding-top: 6px;
    height: 39px;
    width: 100%;
}

.headerLeft > img {
    width: 29px;
    height: 29px;
    background: rgb(162, 162, 162) none repeat scroll 0% 0%;
    border-radius: 100%;
    padding: 2px;
    margin-left: 20px;
}

.headerRight > span {
    margin-right: 15px;
    padding: 4px 8px;
    border-radius: 22px;
    background: rgb(162, 162, 162) none repeat scroll 0% 0%;
    color: white;
}

#container {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

#doctorName {
    font-size: 16px;
}
#hospital {
    font-size: 14px;
}

.description {
    overflow: hidden;
    padding: 10px 0;
}

.description > img {
    width: 64px;
    height: 64px;
    float: left;
    margin-left: 15px;
    border-radius: 100%;
}

.description > div {
    float: left;
    line-height: 32px;
    margin-left: 14px;
}

ul, li {
    list-style: none;
}

ul {
    overflow: hidden;
    width: 100%;
}

li {
    float: left;
    text-align: center;
    line-height: 40px;
}

.doctorType > li {
    width: 33.3%;
    color: #bababa;
}

.doctorLevel > li {
    width: 33.3%;
    color: #222222;
}


.recentTime {
    padding: 0px 15px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    color: #FF813D;
    font-size: 14px;
}

#recentTime > li {
    width: 13%;
    line-height: 23px;
    height: 53px;
    display: inline-block;
    margin: 0px;
    float: none;
    padding: 4px 0 0;
}
#recentTime {
    overflow: hidden;
    font-size: 14px;
    height: 60px;
    width: 100%;
    overflow-x: auto;
    overflow-y: hidden;
    white-space: nowrap;
    margin: 10px 0;
    text-align: center;
    background: url("../images/images_family_doctor/bg.png");
    background-size: 100% 100%;
}

