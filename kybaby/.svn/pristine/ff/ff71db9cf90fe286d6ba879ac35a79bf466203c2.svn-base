@charset "utf-8";

/* ==========================================================================
Author's custom styles
========================================================================== */

/*
* common start
*/
* {
	margin: 0;
	padding: 0;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}

body {
	width: 100%;
	min-width: 320px;
	height: 100%;
	font-size: 1em;
	font-family: 'Hiragino Sans GB',"Microsoft YaHei";
	color: #505050;
	background:#ffffff;
}
body::-webkit-scrollbar{
	display:none;
}
label{
	background:#fff;
	display:inline-block;
	color:#666;
}
ul li,
ol li {
	list-style: none;
}
input{
	font-size:14px;
}
input:focus{
	outline:none;
}
input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
	-webkit-appearance: none;
	appearance: none;
	margin: 0;
}
img {
	border: 0;
	max-width: 100%;
}

a {
	text-decoration: none;
	color: #505050;
}

.float-left {
	float: left;
}

.float-right {
	float: right;
}

.text-left {
	text-align: left;
}

.text-right {
	text-align: right;
}
.color-gray{
	color: #909090;
}
.color-basic{
	color: #ff813d;
}
.gray_s{
	width: 100%;
	height: 1px;
	background: #e8e8e8;
	clear: both;
}
.gray_ss{
	width: 100%;
	height: 2px;
	background: #e8e8e8;
	clear: both;
}
.text-center {
	text-align: center;
}

.dis-none {
	display: none;
}

.dis-block {
	display: block;
}

.red {
	color: red;
}

.white {
	color: white;
}

.black {
	color: black;
}

/*
* common end
*/

#header:before,
#header:after,
#banner:before,
#banner:after,
#container:before,
#container:after,
#footer:before,
#footer:after {
	content: " "; /* 1 */
	display: table; /* 2 */
}

#header:after,
#banner:after,
#container:after,
#footer:after {
	clear: both;
}

/*
* header start
*/
#header {
	width: 100%;
	height: 45px;
	background: #fff;
	text-align: center;
	position: fixed;
	top: 0;
	z-index: 999;
}

#header .tit {
	width: 90%;
	margin:0 auto;
}

#header .header-left,#header .headerLeft {
	width: 50%;
	float: left;
	line-height: 45px;
	height: 45px;
	color: #ff813d;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	text-align: left;
	font-size: 16px;
}
#header .header-left p,#header .headerLeft p {
	background: url("../images/prev_01.png") no-repeat center center;
	background-size: 30px;
	height: 45px;
}
.header-left>span{
	display: inline-block;
	background: url(../images/images_main/return_arrow.png)no-repeat left center;
	background-size: 9px;
	padding-left: 18px;
}
.header-left>div{
	background: url(../images/images_main/return_arrow.png)no-repeat left center;
	background-size: 9px;
	padding-left: 18px;
}
#header_right_button>span{
	display: inline-block;
	background: url(../images/images_main/right_arrow.png)no-repeat right center;
	background-size: 9px;
	padding-right: 18px;
}

#header .header-right {
	width: 50%;
	float: right;
	line-height: 45px;
	height: 45px;
	font-size: 16px;
	text-align: right;
	color: #ff813d;
}
#header .header-right p {
	width: 50px;
	line-height: 26px;
	height: 26px;
	border: 1px solid #ff813d;
	border-radius: 3px;
	margin: 8px auto;
	background: #ff813d;
	color: #fff;
	font-size: 0.875em;
}

.header_tab,.header_tab1 {  /*header_tab*/
	width: 70%;
	margin: 4px auto;
	height: 35px;
	line-height: 35px;
	clear: both;
	overflow: hidden;
	border: 1px solid #ff813d;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	font-size: 0.875em;
}
.header_tab1{
	width: 270px;
}
.header_tab1>div{
	width:90px;
	border-right: 1px solid #ff813d;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	float: left;
}
.header_tab1>div:nth-child(3){
	border-right: 0px solid #ff813d;
}
.header_tab .tab_left {
	float: left;
	width: 50%;
}
.header_tab .tab_right {
	float: right;
	width: 50%;
}
.header_tab .select ,.header_tab1 .select{
	background: #ff813d;
	color: #fff;
}

/*
* header end
*/

/*
* banner start
*/


#banner {
	width: 100%;
	/*height: 150px;*/
}
#banner img {
	/*display: none;*/
}
/*
* banner end
*/



/*
* container start
*/
#container {
	width: 100%;
	margin:45px 0 50px ;
}



/*
* container end
*/

/*
* footer start
*/
#footer {
	width: 100%;
	height: 50px;
	background: #fff;
	position: fixed;
	bottom: 0;
}
#footer ul a {
	display: block;
}

#footer ul li {
	width: 25%;
	height: 45px;
	padding-top: 5px;
	display: block;
	float:left;
	text-align: center;
	color: #c8c8c8;
}
#footer ul li.select {
	color: #ff813d;
}
#footer ul li p:last-child {
	font-size: 0.875em;
}

/*
* footer end
*/
