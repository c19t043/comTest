.order_item_selected{
    color: #1cba93;
}
.item_div{
    width: 120px;
    position: absolute;
    z-index: 1110;
    top:45px;
    right: 5%;
}
#triangle_arrow{
    position: absolute;
    top:34px;
    right: 6%;
    z-index: 1110;
    width:0;
    height:0;
    border-left:8px solid transparent;
    border-right:8px solid transparent;
    border-bottom:11px solid #6BDABF;
}
.item_div>div{
    height: 35px;
    line-height: 35px;
    background: #6BDABF;
    border-bottom: 1px solid #E3F8F3;
    color: #FDFEFC;
    font-size: 14px;
    text-align: center;
}
.hide{
    display: none;
}