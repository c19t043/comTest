.step_1,.step_2{
	background: rgba(0,0,0,0);
}
.step_1 label,.step_2 label{
	width:22%;
	height:50px;
	text-align:left;
	padding-left:5%;
	float:left;
	font-size:14px;
	color:#666;
	line-height:50px;
	background: rgba(0,0,0,0);
}
.step_1 input,.step_2 input{
	width:73%;
  height: 50px;
  text-indent: 10px;
  border: 0;
	background: rgba(0,0,0,0);
}
.step_1 .get_reg_code,
.step_2 .get_reg_code {
	display:inline-block;
}
.next_step,.regist{
	margin-top:20px;
}

#regist_code,
#original_code,
#new_code,
#code {
	width:39%;
}
#regist_baby_boy,#regist_baby_girl{
	width:20px;
	height:20px;
	float:left;
  position: Relative;
  top: 15px;
}
label.sex{
	width:30px;
}
#city{
    display: inline-block;
    height: 50px;
    width: 70%;
    line-height: 50px;
    font-size: 14px;
}
#city select{
	width:32%;
}
#street_address{
	text-indent:25px;
	background:#fff;
	width:100%;
	color:#666;
}
#xxaddress{
	text-indent:25px;
}
p.agreement{
	width: 90%;
	margin: 40px auto 5px auto;
	font-size: 12px;
}
p.agreement input{
	position:relative;
	top:2px;
	margin-right:10px;
}
.mt_20 {
	margin-top: 20px;
}
#doctor_xieyi{
	font-size: 12px;
	color: #909090;
}

#doctor_xieyi_box {
	position:absolute;
	overflow-x:hidden;
	overflow-y:auto;
	top:0px;
	left:0px;
	width:100%;
	height:800px;
	background:#fff;
	z-index:1000;
	display:none;
}

#doctor_xieyi_tit{
	width: 100%;
	height: 45px;
	background: #fff;
	text-align: center;
	position: fixed;
	top: 0;
	z-index: 9199;
}
#doctor_xieyi_tit .a {
	width: 20%;
	float: left;
	line-height: 45px;
	font-size: 30px;
	height: 45px;
	color: #c8c8c8;
}
#doctor_xieyi_tit .a p {
	background: url("../images/prev_01.png") no-repeat center center;
	background-size: 25px;
	height: 45px;
}
#doctor_xieyi_tit .b {
	width: 60%;
	float: left;
	line-height: 45px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	font-size: 18px;
}
#doctor_xieyi_tit .c {
	width: 20%;
	float: right;
	line-height: 45px;
	height:45px;
}
#header{
	color: #1cba93;
	background: rgba(0,0,0,0);
}
#container {
	margin: 50px 10px 50px 0;
}
body{
	width: 100%;
	height: 100%;
	background: url("../images/menuicon/login_bg.png")no-repeat top center;
	background-size: 100%;
}
