/*by yjl*/
#cover{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0px;
    left: 0px;
    background: rgba(0,0,0,0.6);
    font-family: 'Microsoft YaHei';
    z-index: 32000;
}
#content{
    width: 84%;
    position: absolute;
    left: 7%;
    top: 50%;
    background: #fff;
    border-radius: 6px;
}
#content>div.chooseupdate{
    overflow: hidden;
    width: 86%;
    line-height: 34px;
    margin: 0px auto;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 24px;
}
#content>p{
    line-height: 46px;
    font-size: 26px;
    color: #4fc1e9;
    border-bottom: 2px solid #909090;
    margin: 0px;
    text-align: center;
}
#content>div.chooseupdate>div.later{
    float: left;
    width: 44%;
    border-radius: 6px;
    background: #bbb;
    color: #909090;
    text-decoration: underline;
}
#content>div.chooseupdate>div.rightaway{
    float: right;
    width: 44%;
    border-radius: 6px;
    background: #4fc1e9;
    color: #ffffff;
}
#content>div.chooseupdate>div.rightawayspe{
    margin: 0px auto;
    width: 70%;
    border-radius: 6px;
    background: #4fc1e9;
    color: #ffffff;
}
.updatecontent{
    padding: 4px 15px 0px;
}
.updateversion{
    padding: 9px 15px 0px;
}
.updatecontent,.updateversion{
    font-size: 14px;
    color: #505050;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.updatecontent>p,.updateversion>p{
    margin: 10px 0px;
}









