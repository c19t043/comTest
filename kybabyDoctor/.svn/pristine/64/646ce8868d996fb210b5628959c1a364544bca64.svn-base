}

/*个人专栏编辑 editpersonalcolumn*/

.edit_tit,
.edit_article {
	border: none;
	width: 90%;
	padding: 0 5%;
	font-size: 1em;
}
.edit_tit {
	height: 50px;
	line-height: 50px;
}
.edit_article {
