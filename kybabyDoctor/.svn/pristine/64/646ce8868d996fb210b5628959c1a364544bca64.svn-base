	padding-top: 10px;
}

/*推荐奖励 recommend*/
.recommend_box {
	position: relative;
	width: 250px;
	height: 170px;
	margin:10px auto 0 auto;
	/*background: #e5e5e5;*/
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	text-align: center;
}
.recommend_box_top {
	position: absolute;
	top: 50px;
	left: 100px;
	padding: 5px 0;
	/*background: #fff;*/
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
	/*position: relative;*/
}
.recommend_box .user_photo {
	width: 50px;
	height: 50px;
	margin: 0 auto;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	background: #000;
}
.qr_img {
	width: 170px;
	height: 170px;
	margin: 20px auto;
}
.triangle-down {
	width: 0;
	height: 0;
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	border-top: 20px solid white;
	position: absolute;
	bottom: -15px;
	left: 115px;
}
.recommend_code {
	margin: 55px 0 25px 0;
	color: #505050;
	line-height: 40px;
	font-size: 1.125em;
	text-align: center;
}

/*意见反馈 feedback*/
.feedback {
	border: none;
	width: 90%;
	margin:0 auto;
	font-size: 1em;
	height: 200px;
	padding: 5px;
	display: block;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}

