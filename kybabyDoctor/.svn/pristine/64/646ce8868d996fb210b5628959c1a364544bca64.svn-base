	padding-top: 10px;
}

/*我的主页 mypage*/
.mypage {
	width: 90%;
	padding: 10px 5% 20px 5%;
	background: #fff;
	clear: both;
	overflow: hidden;
}
.myphoto {
	width: 60px;
	float: left;
	padding-top: 20px;

}
.myphoto .myphoto_img {
	width: 60px;
	height: 60px;
	/*background: #000;*/
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
}
.myphoto .myphoto_img img {
	width: 100%;
	height: 100%;
	border-radius:50%;
}
.myinfo {
	margin-left: 70px;
}
.myinfo p {
	font-size: 0.875em;
	width: 100%;
	line-height: 22px;
}
.myinfo .my_indent {
	width: 68%;
	text-indent: -32%;
	padding: 0 0 0 32%;
}
.myinfo .myname {
	width: 100%;
	font-size: 1.25em;
	line-height: 24px;
}
.myinfo .small {
	font-size: 0.75em;
}
.mymark {
	color: #c8c8c8;
	width: 100%;
	clear: both;
	overflow: hidden;
}
.mymark .mark_list {
	width: 100%;
	line-height: 30px;
	vertical-align: middle;
	clear: both;
	overflow: hidden;
}
.mymark .mark_list .mark_list_left {
	font-size: 0.875em;
	width: 60px;
	float: left;
	text-align: right;
}
.mymark .mark_list .mark_list_right{
	width: 66%;
	padding-left: 5px;
	float: left;
	text-align: left;
	vertical-align: middle;
}
.mymark .mark_list .mark_list_right img {
	padding-left: 3px;
	width: 16px;
	height: 16px;
	position: relative;
	top: -3px;
}
.mypage_tab {
	width: 100%;
	margin: 10px 0;
	clear: both;
	overflow: hidden;
}
.mypage_tab ul li {
	padding:5px;
	margin: 5px;
	text-align: center;
	float: left;
	font-size: 0.875em;
	color: white;
	background-color: #4fc1e9;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
.bg_white {
	background: #fff;
}
.mypage_service {
	width: 90%;
	margin: 0 auto;
	padding: 20px 0;
	background: #fff;
	overflow-x: auto;
	overflow-y: hidden;
	white-space: nowrap;
}
.service_box {
	width: 130px;
	margin: 0 10px 0 0;
	display: inline-block;
}
.mypage_service::-webkit-scrollbar {
	display: none;
}
.service_box .service_img {
	width: 130px;
	height: 130px;
	/*background: #000;*/
}
.service_box .service_tit {
	line-height: 30px;
	font-size: 0.875em;
	width: 100%;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.service_box .sale {
	width: 60%;
	float: left;
	font-size: 0.75em;
	text-align:left;
}
.service_box .price {
	width: 40%;
	float: right;
	font-size: 0.75em;
	text-align:right;
	color: red;
}
.mypage_personal {
	width: 100%;
	padding: 5px 0;
	background: #fff;
	clear: both;
	overflow: hidden;
}
.mypage_personal_top {
	width: 90%;
	padding: 0 5%;
	clear: both;
	overflow: hidden;
}
.mypage_personal .personal {
	width: 40%;
	float: left;
	font-size: 0.875em;
}
.mypage_personal .release {
	width: 20%;
	float: right;
	border: 1px solid #4fc1e9;
	text-align: center;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	line-height: 25px;
	color: #4fc1e9;
	font-size: 0.875em;
}
.mypage_personal .myarticle {
	width: 90%;
	padding:0 5%;
	margin: 10px 0;
	font-size: 0.875em;
	clear: both;
	overflow: hidden;
}
.mypage_personal .myarticle .article_date,
.mypage_personal .myarticle .article_title {
	line-height: 24px;
}
.mypage_personal .myarticle .article_date {
	width: 75%;
	float: left;
}
.article_state {
	width: 20%;
	float: right;
	text-align: center;
	background: #4fc1e9;
	color: #fff;
	line-height: 24px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
.mypage_personal .myarticle .comment {
	background: url(../images/icon_comment.png) no-repeat left center;
	background-size: 16px;
	width: 30%;
	float: left;
	padding-left: 20px;
	line-height: 30px;
	font-size: 0.75em;
	color: #c8c8c8;
}
.mypage_personal .myarticle .view {
	background: url(../images/icon_view.png) no-repeat left center;
	background-size: 16px;
	width: 30%;
	float: left;
	padding-left: 25px;
	line-height: 30px;
	font-size: 0.75em;
	color: #c8c8c8;
}
.mypage_personal .myarticle .right {
	width: 20%;
	float: right;
	line-height: 30px;
	font-size: 1.5em;
	text-align: center;
	color: #c8c8c8;
}

/*个人专栏 personalcolumn*/
.personal_article {
	width: 90%;
	height: 200px;
	padding: 5px 5%;
	background: #fff;
}
.article_comment {
	width: 90%;
	padding: 0 5%;
	background: #fff;
	clear: both;
	overflow: hidden;
}
.article_comment_list {
	width: 100%;
	margin: 10px 0;
	padding-bottom: 5px;
	border-bottom: 1px solid #c8c8c8;
	clear: both;
	overflow: hidden;
}
.article_comment_left {
	width: 40px;
	height: 40px;
	margin-top: 5px;
	float: left;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	background: #000;
}
.article_comment_left img {
	border-radius: 50%;
    height: 40px;
    width: 40px;
}
.article_comment_right {
	margin-left: 50px;
}
.article_comment_right .comment_username {
	font-size: 1em;
	line-height: 24px;
}
.article_comment_right .comment_date {
	font-size: 0.75em;
	line-height: 24px;
}
.article_comment_right .comment_detail {
	font-size: 0.75em;
