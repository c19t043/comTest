
.logo_box {
    width: 100%;
    text-align: center;
}
input{
    width: 100%;
    height: 20px;
    line-height: 20px;
    padding:15px 0px;
    border:0;
    display: block;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#username{
    width: 100%;
    height: 50px;
    line-height: 22px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border:0;
    display: block;
    background:#fff url(../images/username.png) no-repeat 15% center;
    background-size: 16px;
    color:#8E8E8E;
    text-align: center;
}
#password{
    width: 100%;
    height: 50px;
    line-height: 22px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border:0;
    display: block;
    background:#fff url(../images/password.png) no-repeat 15% center;
    background-size: 16px;
    color:#8E8E8E;
    text-align: center;
}
p.forget_password{
    margin-top:15px;

}
p.com_p{
    width:90%;
    text-align:right;
    margin:5px auto;
    font-size:12px;
    line-height: 2em;
}
.select{
    margin: 15px auto;
    text-align: center;
}
select{
    -webkit-appearance: searchfield;
    -moz-appearance: searchfield;
    color: #8E8E8E;
    border: none;
}
#cover{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0px;
    left: 0px;
    background: rgba(0,0,0,0.6);
    font-family: 'Microsoft YaHei';
    z-index: 32000;
    display: none;
}
#covercontent{
    width: 84%;
    position: absolute;
    left: 7%;
    top: 50%;
    background: #fff;
    border-radius: 6px;
}
#covercontent>div.chooseupdate{
    overflow: hidden;
    width: 86%;
    line-height: 34px;
    margin: 0px auto;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 24px;
}
#covercontent>p{
    line-height: 46px;
    font-size: 26px;
    color: #4fc1e9;
    border-bottom: 2px solid #909090;
    margin: 0px;
    text-align: center;
}
#covercontent>div.chooseupdate>div.later{
    float: left;
    width: 44%;
    border-radius: 6px;
    background: #bbb;
    color: #909090;
    text-decoration: underline;
}
#covercontent>div.chooseupdate>div.rightaway{
    float: right;
    width: 44%;
    border-radius: 6px;
    background: #4fc1e9;
    color: #ffffff;
}
#covercontent>div.chooseupdate>div.rightawayspe{
    margin: 0px auto;
    width: 70%;
    border-radius: 6px;
    background: #4fc1e9;
    color: #ffffff;
}
.updatecontent{
    padding: 4px 15px 0px;
}
.updateversion{
    padding: 9px 15px 0px;
}
.updatecontent,.updateversion{
    font-size: 14px;
    color: #505050;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.updatecontent>p,.updateversion>p{
    margin: 10px 0px;
}
