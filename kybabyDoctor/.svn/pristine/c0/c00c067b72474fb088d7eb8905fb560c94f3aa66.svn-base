
* {
    margin: 0px;
    padding: 0px;
}

body {
    color: #6B6B6B;
    line-height: 34px;
    font-size: 14px;
}

#container, #periodTime, header, #notice {
    padding: 0px 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

#container, #periodTime {
    width: 100%;
    margin-top: 5px;
}

header {
    overflow: hidden;
    width: 100%;
    line-height: 40px;
    margin-top: 50px;
    padding: 0px 20px;
}

#repeat {
    margin-bottom: 60px;
}

#repeat > section {
    overflow: hidden;
    width: 70%;
    margin: 0px auto;
    border: 2px solid #1cba93;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border-radius: 10px;
}

#repeat div {
    width: 50%;
    line-height: 34px;
    float: left;
    color: #1cba93;
    text-align: center;
}

#repeat div:nth-child(1) {
    border-right: 2px solid #1cba93;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

#repeat div.activeStyle {
    background: #1cba93;
    color: white;
}

ul, li {
    list-style: none;
}

ul {
    overflow: hidden;
    margin-bottom: 16px;
}

li {
    width: 30%;
    float: left;
    line-height: 40px;
    text-align: center;
}

li.spe {
    width: 10%;
    border-bottom: 2px solid #c0c0c0;
    margin-top: 60px;
}

li.border {
    border-left: 2px solid #c0c0c0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

li > select {
    padding: 0px 12%;
    font-size: 18px;
    color: #222222;
    border-radius: 8px;
    border: 2px solid #c0c0c0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-shadow: 0px 3px 3px 3px #bababa inset;
    -moz-box-shadow: 0px 3px 3px 3px #bababa inset;
    box-shadow: 0px 3px 3px 3px #bababa inset;
    -webkit-appearance: searchfield;
    -moz-appearance: searchfield;
}
li p{
    font-size: 14px;
}
.goToAddress{
    position: absolute;
    right: 10px;
    top: 0px
}
#notice {
    font-size: 18px;
    overflow: hidden;
}

#notice > div {
    float: left;
}

.spe {
    width: 20%;
    color: #1cba93;
    text-align: center;
}

#notice > div:nth-child(2) {
    width: 60%;
    margin-left: 6%;
}


#footer p{
    height: 43px;
}

#meetAddress, #meetHospital, #isRepeat {
    color: #909090;
}

