.personal_list > div {
    width: 100%;
    padding: 0px 15px 0px 15px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    overflow: hidden;
}

.personal_list .listaddress>input {
    border: 1px solid #CCCCCC;
    width: 96%;
    /*-webkit-box-shadow: 0px -1px 1px 0px #ccc;*/
    /*-moz-box-shadow: 0px -1px 1px 0px #ccc;*/
    /*box-shadow: 0px -1px 1px 0px #ccc;*/
    height: 30px;
    border-radius: 4px;
    /*text-align: center;*/
    float: left;
    padding: 0 10px;
}
.listaddress>input:nth-child(1){
    background: url(../images/iconfont-sousuo1.png)no-repeat 98.5%;
}
.personal_list .listaddress>.appDate1{
    width: 44%;
    height: 35px;
    background: url(../img/calendar.png)no-repeat 96%;
    background-size: 24px;
}
.personal_list >.print{
    position: fixed;
    bottom: 0;
    left: 0;
    height: 50px;
    background: #4FC1E9;
    line-height: 50px;
    font-size: 18px;
    color: #ffffff;
}
.personal_list >.reset{
    width: 41%;
    float: left;
    line-height: 34px;
    background: #4FC1E9;
    color: #ffffff;
    border-radius: 4px;
    margin-left: 6%;
    padding: 0px;
}
.personal_list >.historylist,.personal_list >.search{
    width: 44%;
    float: left;
    line-height: 37px;
    background: #4FC1E9;
    color: #ffffff;
    border-radius: 4px;
    margin-left: 4%;
    padding: 0px;
}
.personal_list >.reset{
    background: #cccccc;
    color: #909090;
}
.speblue {
    background: #4FC1E9;
    color: #ffffff;
}

#footer > div {
    width: 20%;
    margin-left: 30px;
    text-align: left;
}
