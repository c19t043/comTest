    <style>
        #header {
            width: 100%;
            height: 45px;
            background: #1cba93;
            text-align: center;
            position: fixed;
            top: 0;
            z-index: 999;
            color: white;
        }

        #header .header-left {
            width: 20%;
            float: left;
            line-height: 45px;
            font-size: 30px;
            height: 45px;
        }

        #header .header-left p, #header .headerLeft p {
            background: url("images/normalicon/return.png") no-repeat center center;
            background-size: 18px;
            height: 45px;
        }

        #header .header-center {
            width: 60%;
            float: left;
            line-height: 45px;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
            font-size: 16px;
        }

        #header .header-right {
            width: 20%;
            float: right;
            line-height: 45px;
            height: 45px;
        }

        /*#header .header-right div {*/
            /*width: 100%;*/
            /*background: url("images/consulting/healthrecord.png") no-repeat center center;*/
            /*background-size: 18px;*/
            /*height: 45px;*/
        /*}*/
        #header .header-right span{
            margin: 0 2px;
            display: inline-block;
            width: 4px;
            height: 4px;
            border-radius: 4px;
            border: 1px solid #fff;
        }
        #more{
            display: inline-block;
            width: 12%;
            height: 50px;
            float: left;
            background: url("images/consulting/more.png") no-repeat center center;
            background-size: 21px;
        }
        #showBusiness{
            position: fixed;
            left: 0;
            bottom: 0;
            background: #f0f0f0;
            width: 100%;
        }
        #showBusiness ul{
            overflow: hidden;
            padding: 0 15px;
        }
        #showBusiness ul li{
            width: 25%;
            float: left;
            padding: 5px 0;
            text-align: center;
            font-size: 12px;
            line-height: 26px;
        }
        #showBusiness ul li img{
            width: 40px;
        }
        #sendImg{
            display: block;
            width: 40px;
            height: 40px;
            position: relative;
            top: -40px;
            opacity: 0;
            z-index: 2;
        }
        .hi{
            width: 100%;
            height: 100%;
            position: fixed;
            top: 45px;
            left: 0;
            background: white;
            z-index: 1000;
            overflow: auto;
        }
        .hi input{
            border: none;
        }
        .hi input{
            background-color:expression((this.readOnly && this.readOnly == true)? "#ffffff":"");
        }
        .hi input[readonly]{
            background-color: white;
        }
        .text img{
            width: 26px;
            margin-right: 10px;
        }
        .zzc{
            line-height: 34px;
            padding: 0 16px;
        }
        .zzc_t1{
            display: inline-block;
            width: 100px;
        }
        .hidden_menu{
            height: 0;
            overflow: hidden;
            background: #1cba93;
        }
        .hidden_menu>li{
            border-top: 1px solid #ffffff;
        }
    </style>
</head>
