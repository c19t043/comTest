
* {
    margin: 0px;
    padding: 0px;
}

header {
    background: #F7F7F7;
    overflow: hidden;
    padding-top: 6px;
    height: 39px;
    width: 100%;
}

.headerLeft {
    float: left;
    width: 40%;
}

.headerRight {
    width: 40%;
    float: right;
    padding-top: 5px;
    text-align: right;
}
.header-right div{
    float: right;
    line-height: 24px;
    width: 60px;
    border-radius:6px ;
    background: #909090;
    color: #ffffff;
    margin: 10px;
}
#share{
    position: fixed;
    top: 0px;
    left: 0px;
    z-index: 10000;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
}
#share img{
    margin-top: 50px;
}
.headerLeft > img {
    width: 29px;
    height: 29px;
    background: rgb(162, 162, 162) none repeat scroll 0% 0%;
    border-radius: 100%;
    padding: 2px;
    margin-left: 20px;
}

.headerRight > span {
    margin-right: 15px;
    padding: 4px 8px;
    border-radius: 22px;
    background: rgb(162, 162, 162) none repeat scroll 0% 0%;
    color: white;
}

#container {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

#doctorName {
    color: #222222;
    font-size: 16px;
}
#doctorMessage{
    margin-left: 15px;
    text-align: left;
}
.description {
    padding: 15px;
    overflow: hidden;
}
.description>.float-left{
    text-align: center;
}
.description>.float-left> img {
    width: 70px;
    height: 70px;
    border-radius: 100%;
}

.description >.float-left> div {
    padding-top: 12px;
}
.description>.float-right{
    width: 50%;
}
#erweima{
    position: relative;
    margin: 0 auto;
    text-align: center;
}
#hospital {
    color: #909090;
    font-size: 13px;
}

ul, li {
    list-style: none;
}

ul {
    overflow: hidden;
    width: 100%;
}

li {
    float: left;
    text-align: center;
    line-height: 40px;
}

.doctorType > li {
    width: 33%;
    line-height: 15px;
    margin-top: 18px;
    border-left: 1px solid #e8e8e8;
}
.doctorType > li:nth-child(1){
    border-left: none;
}

.doctorLevel > li {
    width: 33.3%;
    color: #1CBA93;
    font-size: 13px;
    margin: 2px 0 4px 0;
}

#recentTime > li {
    width: 14.2%;
    border-radius: 100%;
    line-height: 25px;
    height: 50px;
}

.recentTime {
    padding: 10px 15px 0px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

#recentTime {
    background: #1CBA93;
    color: #D3E5FC;
    margin: 10px 0px;
    padding: 15px 0px;
    font-size: 14px;
}

.doctorAppoint {
    color: white;
}

.activeAppoint {
    color: #1CBA93;
    background: white;
    border-radius: 100%;
}

.areaAppoint {
    color: #EE5F66;
}

.address {
    height: 48px;
    width: 100%;
    overflow: hidden;
}

.addressImg {
    width: 10%;
    margin-left: 5%;
    float: left;
    height: 48px;
}

.addressDes {
    float: left;
    height: 48px;
    width: 70%;
    margin-left: 5%;
}

.addressDes > div {
    display: table;
    width: 100%;
}

.addressImg > img {
    width: 100%;
    height: 100%;
}

.addressDes > div > address {
    width: 100%;
    height: 48px;
    vertical-align: middle;
    display: table-cell;
}

#doctorIntroduction {
    padding: 0px 15px 10px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    color: #505050;
}

#doctorIntroduction > p {
    color: #909090;
    margin-top: 11px;
}

#doctorIntroduction > div.description {
    font-size: 14px;
    color: #bababa;
    text-indent: 2em;
    line-height: 25px;
}

small > span {
    color: #EE5F66;
}

.preDirection span {
    padding: 1px 2px;
    font-size: 10px;
    border: 1px solid #6FD3BB;
    border-radius: 5px;
    display: inline-block;
}

.header-left div {
    background: url("../images/prev_01.png") no-repeat center center;
    background-size: 25px;
    height: 45px;
}
.professional,.goodat{
    margin-top: 5px;
    overflow: hidden;
    color: #909090;
    line-height: 26px;
    padding: 3px 15px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.professional>div:nth-child(1),.goodat>div:nth-child(1){
    width: 70px;
    float: left;
    line-height: 30px;
}
#professional,#goodat{
    float: left;
    margin-left: 12px;
    width: 66%;
    color: #505050;
}
#professional>span,#goodat>span{
    display: inline-block;
    border-radius: 4px;
    padding: 0px 5px;
    color: #1CBA93;
    font-size: 13px;
}
#footer{
    width: 100%;
    height: 70px;
}
#footer>img{
    width: 100%;
    height: 100%;
}