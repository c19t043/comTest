    background-size: 21px;
}

#showBusiness {
    position: fixed;
    left: 0;
    bottom: 0;
    background: #f0f0f0;
    width: 100%;
}

#showBusiness ul {
    overflow: hidden;
    padding: 0 15px;
}

#showBusiness ul li {
    width: 25%;
    float: left;
    padding: 5px 0;
    text-align: center;
    font-size: 12px;
    line-height: 26px;
}

#showBusiness ul li img {
    width: 40px;
}

#sendImg {
    display: block;
    width: 40px;
    height: 40px;
    position: relative;
    top: -40px;
    opacity: 0;
    z-index: 2;
}

.text img {
    width: 26px;
    margin-right: 10px;
}
#illnessImpression{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.3);
    z-index: 10000;
}
#illness_impression_content{
    background: white;
    width: 94%;
    height: 92%;
    position: absolute;
    left: 3%;
    top:4%;
}
#illness_impression_content>.top{
    padding: 10px;
    overflow: hidden;
    color: #1cba93;
    font-size: 14px;
    line-height: 24px;
}
#illness_impression_content>.top>.text-left{
    float: left;
    font-size: 22px;
    width: 20%;
}
#illness_impression_content>.top>.text-center{
    float: left;
    width: 60%;
}
#illness_impression_content>.top>.text-right{
    float: left;
    width: 20%;
}
#illness_impression_content>.top>.text-right>span{
    padding: 3px 12px;
    border-radius: 14px;
    font-size: 12px;
    border: 1px solid #1cba93;
}
#search{
    padding: 10px 24px;
}
#search>input{
    border: 1px solid #d5d5d5;
    width: 90%;
    line-height: 20px;
    border-radius: 40px;
    background: url("../images/iconfont-sousuo1.png") no-repeat;
    background-size: 17px 17px;
    background-position: 12px center;
    padding: 6px 0 6px 10%;
}
#hasChooseTag,#canChooseTag{
    padding: 0 18px;
}
#hasChooseTag>p,#canChooseTag>p{
    color: #1cba93;
    line-height: 36px;
}
#canChooseTagList,#hasChooseTagList{
    overflow: auto;
    padding-bottom: 8px;
}

#canChooseTagList>span,#hasChooseTagList>span{
    padding: 3px 8px;
    font-size: 12px;
    display: inline-block;
    border: 1px solid #e8e8e8;
    border-radius: 20px;
    margin: 8px 0 0 18px;
}
#hasChooseTagList>span{
    background: #1cba93;
    color: white;
    border: 1px solid #1cba93;
}
#canChooseTagList>span.selected{
    border: 1px solid #1cba93;
}