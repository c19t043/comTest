footer{
    display: inline-block;
    width: 100%;
    max-height: 120px;
    background-color: #f0f0f0;
    position: fixed;
    bottom: 0px;
    min-height: 50px;
}
.before img{
    width: 30px;
  margin-top: 10px;
    margin-left: 10px;
}
/*.after img{*/
    /*width: 30px;*/

/*}*/
footer input{
    width: 100%;
    height: 30px;
    margin-top: 10px;
    border: none;
    background: #f0f0f0;
    border-bottom: 1px solid #1cba93;
}
.before{
    display: inline-block;
    width: 10%;
    height: 50px;
    float: left;
}
.after{
    display: inline-block;
    float: right;
    width: 12%;
    text-align: center;
    /* background: url("../images/consulting/send.png") no-repeat center center; */
    /* background-size: 22px; */
    height: 25px;
    line-height: 25px;
    background: #1CBA93;
    color: #fff;
    border-radius: 10px;
    min-width: 25px;
    margin: 12px 7px 6px 0;
    font-size: 12px;
}
.con{
    display: inline-block;
    width: 70%;
    max-height: 120px;
    float: left;
}
.con>#msgText{
    width: 100%;
    margin-top: 10px;
    border: none;
    background: #f0f0f0;
    border-bottom: 1px solid #1cba93;
    min-height: 20px;
    max-height: 80px;
    line-height: 20px;
    _height: 80px;
    margin-left: auto;
    margin-right: auto;
    padding: 3px;
    outline: 0;
    font-size: 12px;
    word-wrap: break-word;
    overflow-x: hidden;
    overflow-y: auto;
    -webkit-user-modify: read-write-plaintext-only;
}

.after span{
    display: inline-block;
    width: 50%;
    height: 30px;
    line-height: 30px;
    color: white;
    background-color:  #50c1e9;
    border-radius: 5px;
    text-align: center;
    margin-top: 10px;
    position: relative;
    top: 1px;
}

/*new*/
.consultop {
    width: 90%;
    padding: 0 5%;
    background: #fff;
}
.consultop_time {
    width: 80%;
    margin: 10px auto;
    text-align: right;
    color: #c8c8c8;
    line-height: 1.5em;
    font-size: 1em;
}
.consultop_tab {
    width: 80%;
    margin: 0 auto;
    clear: both;
    overflow: hidden;
}
.consultop_tab p {
    padding: 5px;
    margin: 2px 3px;
    float: right;
    color: #969696;
    background: #f7f7f7;
    line-height: 1.5em;
    font-size: 0.875em;
    border-radius: 5px;
}
.consultop_img {
    width: 80%;
    margin: 10px auto;
    clear: both;
    overflow: hidden;
}
.consultop_img img {
    width: 200px;
    height: 100px;
    background: red;
    float: right;
}
.user_info {
    clear: both;
    overflow: hidden;
}
.user_txt {
    max-width: 70%;
    margin-top: 5px;
    padding: 5px 2%;
    background: #4fc1e9;
    color: #fff;
    float: right;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    font-size: 0.875em;
    line-height: 1.5em;
}
.user_photo {
    width: 15%;
    float: right;
}
.user_photo img {
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    width: 40px;
    height: 40px;
}


