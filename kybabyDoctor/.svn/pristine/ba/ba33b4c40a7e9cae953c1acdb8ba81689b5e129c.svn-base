/*字体声明*/
@font-face {font-family: 'iconfont';
    src: url('../iconfont/iconfont.eot'); /* IE9*/
    src: url('../iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('../iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
    url('../iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('../iconfont/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
/*定义字体样式*/
.iconfont{
    font-family:"iconfont";
    font-size:28px;font-style:normal;
    color: #c8c8c8;
}
#content{
    min-height: 100%;
    background: #fff;
    /*position: absolute;*/
    /*top: 0;*/
    /*left: 0;*/
    /*z-index: 200;*/
}
#leftNav{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 111;
    width: 240px;
}
#leftNav ul{
    overflow: auto;
}
#leftNav li{
    width: 240px;
    height: 45px;
    line-height: 45px;
    border-bottom: 1px solid #dedede;
    text-align: center;
}
#mask{
    position: absolute;
    bottom: 0;
    left: 0;
    top:0;
    right: 0;
    z-index: 1000;
    width: 100%;
    /*height: 1000%;*/
    min-height: 100%;
    background: rgba(0,0,0,0.5);
    display: none;
    box-shadow: -4px 0 4px rgba(0,0,0,.5),4px 0 4px rgba(0,0,0,.5);
    -webkit-tap-highlight-color: transparent;
}
#exit{
    position: fixed;
    left: 0;
    bottom: 0;
    width: 240px;
    text-align: center;
}
#mask_header{
    padding: 30px 20px;
    overflow: hidden;
    background: #11777C;
    color: #fff;
}
#community_name{
    font-size: 18px;
}
/*--------------------------------------*/
body {
    width: 100%;
    height: 100%;
}
.speblue {
    background: #4FC1E9;
    color: #ffffff;
}
#loginName{
    margin-top: 15px;
    font-size: 15px;
}
#orgOutLogin{
    height: 50px;
    background: #11777C;
    line-height: 50px;
    color: #fff;
}
#mask_bottom>p{
    text-align: center;
    border-top:1px solid #dedede;
    border-bottom:1px solid #dedede;
    position: fixed;
    left: 0px;
    bottom: 50px;
    width: 240px;
    line-height: 40px;
    background: #fff;
}
#cover{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0px;
    left: 0px;
    background: rgba(0,0,0,0.6);
    font-family: 'Microsoft YaHei';
    z-index: 32000;
    display: none;
}
#covercontent{
    width: 84%;
    position: absolute;
    left: 7%;
    top: 50%;
    background: #fff;
    border-radius: 6px;
}
#covercontent>div.chooseupdate{
    overflow: hidden;
    width: 86%;
    line-height: 34px;
    margin: 0px auto;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 24px;
}
#covercontent>p{
    line-height: 46px;
    font-size: 26px;
    color: #4fc1e9;
    border-bottom: 2px solid #909090;
    margin: 0px;
    text-align: center;
}
#covercontent>div.chooseupdate>div.later{
    float: left;
    width: 44%;
    border-radius: 6px;
    background: #bbb;
    color: #909090;
    text-decoration: underline;
}
#covercontent>div.chooseupdate>div.rightaway{
    float: right;
    width: 44%;
    border-radius: 6px;
    background: #4fc1e9;
    color: #ffffff;
}
#covercontent>div.chooseupdate>div.rightawayspe{
    margin: 0px auto;
    width: 70%;
    border-radius: 6px;
    background: #4fc1e9;
    color: #ffffff;
}
.updatecontent{
    padding: 4px 15px 0px;
}
.updateversion{
    padding: 9px 15px 0px;
}
.updatecontent,.updateversion{
    font-size: 14px;
    color: #505050;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.updatecontent>p,.updateversion>p{
    margin: 10px 0px;
}
#contentNew{
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 999;
    width: 100%;
    background: #fff;
    min-height: 100%;
}