<script>
    $(function(){
        $.ajax({
            type: 'post',
            url: urlWay.doctorDataGather+'doctorDataGather.action',
            cache: false,
            async: true,
            data: {
                action: "querySignDoctor",
                professionFlag: "医生",
            },
            success: function (result) {
                if (result.mes == '请登录') {
                    ale('请登录', '24px');
                    link_to("login.html");
                }
                else if (result.mes == "成功") {
                    if(result.signDoctors!=null){
                        for(var i=0;i<result.signDoctors.length>0;i++){
                            var signDoctors = result.signDoctors[i];
                            $("#both_info").append('<div class="doc_list">' +
                                    '<div class="doc_title">' +
                                    '<div class="signing_time">'+signDoctors.time+'</div><div class="signing_num">数量：'+signDoctors.count+'</div></div>' +
                                    '<ul></ul></div>');
                            if(signDoctors.objs!=null) {
                                for(var j=0;j<signDoctors.objs.length;j++){
                                    var doctorInfo = signDoctors.objs[j];
                                    var button_div = '';
                                    if(doctorInfo.authentication=="未提交"||doctorInfo.authentication=="驳回"){
                                        button_div = '<p class="s_btn">提交审核</p>';
                                    }
                                    var registerTime = doctorInfo.registerTime.substring(5,10);
                                    $("#both_info .doc_list").eq(i).find('ul').append('<li class="flex_cont flex_simple">' +
                                            '<div class="doc_information"><p class="doc_name">'+doctorInfo.doctorName+'</p><p class="doc_time">'+registerTime+'</p></div>' +
                                            '<div class="flex_item doc_work"><p class="doc_hospital">'+doctorInfo.doctorEmployer+'</p><p class="doc_position"><span>'+doctorInfo.doctorTitle+'</span><span>'+doctorInfo.department+'</span></p></div>' +
                                            '<div>'+button_div+'<p class="signing_state">'+doctorInfo.authentication+'</p></div></li>');
                                }
                            }
                        }
                        $(".header_nav>.flex_item").click(function(){
                            $(this).addClass("header_nav_select").siblings(".flex_item").removeClass("header_nav_select");
                            for(var i=0;i<$(".doc_list").length;i++){
                                var flag = 0;
                                $(".doc_list").eq(i).show();
                                if($(this).text()=="全部"){
                                    for(var j=0;j<$(".doc_list").eq(i).find("li").length;j++){
                                        var $div = $(".doc_list").eq(i).find("li").eq(j);
                                        $div.show();
                                        flag ++;
                                    }
                                }else if($(this).text()=="已申请"){
                                    for(var j=0;j<$(".doc_list").eq(i).find("li").length;j++){
                                        var $div = $(".doc_list").eq(i).find("li").eq(j);
                                        $div.hide();
                                        if($div.find(".signing_state").text()=="已申请"){
                                            $div.show();
                                            flag ++;
                                        }
                                    }
                                }else if($(this).text()=="已通过"){
                                    for(var j=0;j<$(".doc_list").eq(i).find("li").length;j++){
                                        var $div = $(".doc_list").eq(i).find("li").eq(j);
                                        $div.hide();
                                        if($div.find(".signing_state").text()=="已通过"){
                                            $div.show();
                                            flag ++;
                                        }
                                    }
                                }else if($(this).text()=="驳回"){
                                    for(var j=0;j<$(".doc_list").eq(i).find("li").length;j++){
                                        var $div = $(".doc_list").eq(i).find("li").eq(j);
                                        $div.hide();
                                        if($div.find(".signing_state").text()=="驳回"){
                                            $div.show();
                                            flag ++;
                                        }
                                    }
                                }
                                if(flag == 0){
                                    $(".doc_list").eq(i).hide();
                                }
                                $(".doc_list").eq(i).find(".signing_num").text("数量："+flag);
                            }
                        });
                    }
                } else {
                    ale(result.mes);
                }
            },
            error: function () {
                layer();
            }
        });
    });
</script>
