body{
    background: #f4f4f4;
}
.flex_cont{
    /*父元素*/
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.flex_item{
    /*子元素*/
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    width: 0%;
}
.flex_simple {
    /*定义子元素垂直居中*/
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.right_topButton{
    padding: 4px 10px;
    border: 1px solid #fff;
    border-radius: 10px;
    color: #fff;
}
.s_btn{
    padding: 6px 10px;
    border: 1px solid #1CBA93;
    border-radius: 12px;
    color: #1cba93;
}
.s_icon,.s_text{
    display: block;
    width: 20px;
    flex: none;
}
.n_icon{
    background: url("../images/menu/next.png")no-repeat right center;
    background-size: 9px;
}
.green{
    color:#1CBA93;
}
/*导航*/
.header_nav{
    padding: 16px 0;
}
.header_nav>li.interval{
    height: 16px;
    overflow: hidden;
    color: #e8e8e8;
    margin-top: 2px;
}
.header_nav>li.flex_item{
    text-align: center;
    line-height: 20px;
}
.header_nav_select{
    color: #1cba93;
    font-size: 16px;
}
/*搜索框*/
.search_div{
    background: #fff;
    padding: 14px 18px;
}
.search_div>div{
    padding: 0 14px;
    height: 30px;
    border: 1px solid #e8e8e8;
    border-radius: 50px;
}
.search_div input{
    border: none;
    text-align: center;
}
.search_div span{
    display: inline-block;
    width: 30px;
    height: 30px;
    background: url('../images/menuicon/search.png')no-repeat center;
    background-size: 18px;
}
/*签约医生列表*/
.doc_title{
    overflow: hidden;
    padding: 12px 18px;
    background: #f4f4f4;
}
.doc_title>div{
    display: inline-block;
    width: 50%;
    font-size: 14px;
}
.signing_time{
    float: left;
}
.signing_num{
    float: right;
    text-align: right;
}
.doc_list li{
    padding: 20px 18px;
    border-bottom: 1px solid #e8e8e8;
    background: #fff;
}
.doc_information{
    width: 60px;
    line-height: 30px;
}
.doc_work{
    margin-left: 18px;
    line-height: 30px;
}
.doc_name{
    font-size: 16px;
}
.doc_time{
    font-size: 13px;
    color: #909090;
}
.doc_position>span{
    display: inline-block;
    margin-right: 12px;
    font-size: 13px;
}
.signing_state{
    margin-top: 8px;
    text-align: center;
    font-size: 14px;
}
/*点切换*/
.choose_ul{
    overflow: hidden;
}
.choose_ul>li{
    display: inline-block;
    float: left;
    width: auto;
    line-height: 55px;
}
.c_selected>.select_option,.c_selected>.d_select_option {
    background: #1cba93;
}
.select_option {
    border-radius: 6px;
}
.d_select_option,.select_option{
    border: 1px solid #1cba93;
    width: 6px;
    height: 6px;
    display: inline-block;
    vertical-align: 2px;
}
.choose_item {
    margin: 0 38px 0 10px;
    display: inline-block;
    font-size: 14px;
}
/*医生资料*/
.information_div_img{
    padding: 8px 18px;
    background: #fff;
}
.information_div{
    padding: 0 18px;
    background: #fff;
}
.information_div>p,.information_div_img>p,.s_text{
    line-height: 55px;
}
.layer_choose>p{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.information_div>p.text{
    padding: 15px 0;
    font-size: 14px;
    line-height: 26px;
}
.information_div>label,.information_div_img>label{
    display: block;
    width: 100px;
    line-height: 55px;
}
.information_div>label#referees{
    display: block;
    width: 150px;
}
.information_div input,.information_div_img input{
    width: 100%;
    line-height: 20px;
    padding: 15px 0;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: #fff;
    outline: none;
    border: none;
}
.h_img{
    margin: 8px 10px 0 0;
    width: 40px;
    height: 40px;
    background: #f4f4f4;
    overflow: hidden;
}
p.info_title{
    line-height: 35px;
    text-align: center;
    color: #909090;
}
#doc_service{
    padding: 12px 18px;
}
#doc_service span{
    margin: 6px 4px;
    padding: 3px 5px;
    display: inline-block;
    border: 1px solid #909090;
    border-radius: 3px;
    color: #909090;
    font-size: 13px;
}
#doc_service span.selected{
    border: 1px solid #1CBA93;
    background: #1CBA93;
    color: #fff;
}