.message_box{
    width: 100%;
    overflow: hidden;
    color: #909090;
    text-align: center;
    font-size: 14px;
}
.gray_s{
    margin: 7px 0px;
    height: 4px;
    width: 100%;
    background: #ffffff;
    float: left;
}
.left_message{
    float: left;
    width: 69%;
}
.left_message>div{
    width: 100%;
    overflow: hidden;
}
.point_time{
    float: left;
    width: 22%;
