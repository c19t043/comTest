#headerL {
    width: 100%;
    height: 45px;
    background: #fff;
    text-align: center;
    position: fixed;
    top: 0;
    z-index: 999;
}

#headerL .header-left{
    width: 20%;
    float: left;
    line-height: 45px;
    font-size:30px;
    height:45px;
    color: #c8c8c8;
}

#headerL .header-left p{
    background: url("../images/normalicon/return.png") no-repeat center center;
    background-size: 20px;
    height: 45px;
}

#headerL .header-center {
    width: 60%;
    float: left;
    line-height: 45px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-size:18px;
}
#headerL .header-right {
    width: 20%;
    float: right;
    line-height: 45px;
    height:45px;
}
#headerL.doctor_header{
    background: #1cba93;
    color:#fff;
}