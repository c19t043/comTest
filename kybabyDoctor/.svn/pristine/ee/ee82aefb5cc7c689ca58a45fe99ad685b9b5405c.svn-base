    line-height: 24px;
    border-radius: 5px;
    margin-bottom: 4px;
}
.point_time img{
    width: 24px;
}
.point_date{
    float: left;
    width: 77%;
    margin-left: 1%;
    line-height: 24px;
    font-size: 10px;
    margin-bottom: 10px;
    text-align: left;
}
.left_message h3{
    color: #1cba93;
}
.right_message{
    float: left;
    width: 29%;
    margin-left: 2%;
}
.right_message img{
    width: 46px;
    margin-top: 4px;
    border-radius: 100%;
}
/*内容2*/
#content2{
    width: 100%;
    padding: 0px 15px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
table{
    width: 100%;
    font-size: 13px;
    color: #505050;
    text-align: center;
}
th:nth-child(1){
    width: 34%;
}
th:nth-child(2){
    width: 32%;
}
th:nth-child(3){
    width: 20%;
}
th:nth-child(4){
    width: 14%;
}
.blue_spe{
    color: #1cba93;
    font-size: 15px;
}
tr{
    height: 40px;
}
td{
}
td:nth-child(4){
    border-right: 1px solid white;
}
span.spe{
    padding: 3px 5px;
    background: #1cba93;
    color: white;
    border-radius: 5px;
}
/*cover*/
#cover{
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background:white;
    z-index: 1000;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    overflow: hidden;
}
#cover div.info{
    float: left;
    text-align: center;
    margin-bottom: 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    line-height: 26px;
}
#cover div.info:nth-child(1){
    width: 44%;
}
#cover div.info:nth-child(1)>span{
    font-size: 14px;
}
#cover div.info:nth-child(2){
    width: 56%;
}
#cover div.info:nth-child(2)>span{
    font-size: 12px;
}
#cover>p{
    font-size: 12px;
    color: #909090;
    padding: 0 15px;
}
.go_work,.after_work{
    width: 44%;
    position: fixed;
    bottom: 60px;
    line-height: 46px;
    font-size: 22px;
    color: white;
    background: #1cba93;
    text-align: center;
    border-radius: 6px;
}
.go_work{
    left: 3%;
}
.after_work{
    right: 3%;
}
