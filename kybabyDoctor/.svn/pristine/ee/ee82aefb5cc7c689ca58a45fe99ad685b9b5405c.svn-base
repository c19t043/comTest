#container{
    position: relative;
}
.select_menu{
    width: 96%;
    margin: 9px auto;
    color: #505050;
    position: fixed;
    left: 2%;
    top: 36px;
    background: #f7f7f7;
}
.select_menu ul{
    list-style: none;
    overflow: hidden;
    width: 300px;
    border:2px solid #1cba93;
    border-radius: 6px;
    margin: 0px auto;
}
.select_menu li{
    font-size: 12px;
    line-height: 35px;
    text-align: center;
    float: left;
    width: 100px;
}
.select_menu li:nth-child(1),.select_menu li:nth-child(2){
    width: 98px;
    border-right: 2px solid #1cba93;
}

.bluespe{
    background: #1cba93;
    color: white;
}
.ul_box{
    width: 100%;
    height: 50px;

    color: #000000;
    margin-top: 58px;
}
.ul_box ul{
    list-style: none;
    width: 100%;
}
.ul_box li{
    float: left;
    width: 20%;
    height: 28px;
    line-height: 28px;
    text-align: center;
    font-size: 0.875em;
    margin-left: 4%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    color: #909090;
    background: #eeeeee;
    border-radius: 6px;
}
.ul_box ul li.blue{
    color: white;
    background: #1cba93;
    border-bottom: 2px solid  #1cba93;
    border-radius: 6px;
}
#ul_cli li{
    width: 17.6%;
    margin-left: 2%;
}

