*{
    margin:0px;
    padding:0px;
    font-size:14px;
}
body{
    min-width:1200px;
}
/*-----头部样式------*/
.headcon{
    width:100%;
    height:40px;
    background:rgb(74, 139, 194);
    color:#fff;
    font-weight:bold;
    font-size:14px;
    line-height:40px;
}
.headcon .head_logo{
    display:inline-block;
    float:left;
    height:40px;
    width:200px;
    margin-left:100px;
}
.headcon .head_con{
    display:inline-block;
    float:right;
}
.headcon .head_user,.headcon .head_out{
    display:inline-block;
    height:40px;
    cursor:pointer;
}
.headcon .head_user{
    width:180px;
    margin-right:20px;
}
.headcon .head_out{
    width:80px;
    margin-right:120px;
    text-indent:40px;
    background:url(../images/ico/log_out.png) no-repeat left 70%;
}
#head_name{
    color:#fdb42b;
}
/*-----内容样式------*/
.middlecon{
    width:100%;
}
.middlecon .leftcon{
    width:17%;
    background: #505050;
    float:left;
    overflow:auto;
}
.middlecon .rightcon{
    width:83%;
    float:left;
    overflow:auto;
}
.middlecon .rightcon .content{
    width:98%;
    margin:auto;
}
.content .con_position{
    width:100%;

}
.content .con_tit{
    height:50px;
    line-height:50px;
    font-weight:bold;
    font-size:24px;
    color:#000;
}
.content .con_meu{
    background:#eee;
    color:#444;
    height:37px;
    line-height:37px;
    font-size:12px;
    text-indent:5px;
}
.content .table_title{
    width:100%;
    height:40px;
    color:#fff;
    background:#4a8bc2;
}
.content .table_h{
    height:420px;
    border-left:1px solid #4a8bc2;
    border-right:1px solid #4a8bc2;

}
.content .table_content{
    width:100%;

    color:#666;
    border-collapse: collapse;
}
.content .table_content tr{
    height:35px;
    cursor:pointer;
}
.content .table_content tr:nth-of-type(odd){
    background:#f1f1f1;
}
.content .table_title th,.content .table_content td{
    text-align:center;
}
.content .table_content td{
    height:35px !important;
}
