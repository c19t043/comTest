@media screen and (min-width: 670px){
    .content .con_page{
        height:40px;
        background:#4a8bc2;
        margin-bottom:20px;
        text-align:center;
        position:relative;
    }
    .content .con_page .a{
        display:inline-block;
        height:40px;
        width:120px;
        line-height:40px;
        font-size:11px;
        text-align:center;
        color:#fff;
        position:absolute;
        top:0px;
        left:0px;
    }
    .content .con_page .a .con_num{
        color:orange;
    }
    .content .con_page .button_cli{
        display:inline-block;
        margin:4px 120px;
    }
    .content .con_page .button_cli .button{
        display:inline-block;
        width:50px;
        height:30px;
        font-size:12px;
        line-height:30px;
        text-align:center;
        border-radius:6px;
        background:#377bd9;
        color:#fff;
        cursor:pointer;
    }
    .content .con_page .button_cli .button:hover{
        background:#1c83d9;
    }
    .content .con_page .button_cli .num{
        display:inline-block;
        width:50px;
        height:30px;
        font-size:14px;
        line-height:30px;
        text-align:center;
        color:#fff
    }
    .content .con_page .page_go{
        display:inline-block;
        width:270px;
        position:absolute;
        top:0px;
        right:16px;
    }
    .content .con_page .page_go .b{
        display:inline-block;
        height:40px;
        width:80px;
        line-height:40px;
        font-size:11px;
        text-align:center;
        color:#fff;
    }
    .content .con_page .page_go .button{
        display:inline-block;
        width:50px;
        height:30px;
        font-size:14px;
        line-height:30px;
        text-align:center;
        border-radius:6px;
        background:#377bd9;
        color:#fff;
        cursor:pointer;
    }
    .content .con_page .page_go .button:hover{
        background:#1c83d9;
    }
    .content .con_page .page_go .page_num{
        color:orangered;
    }
    .content .con_page .page_go .c{
        font-size:11px;
        text-align:center;
        color:#fff;
    }
    .content .con_page .page_go .c input{
        width:50px;
    }
}
@media screen and (max-width: 669px){
    .con_page{
        background: #4A8BC2;
        color:#fff;
        overflow: hidden;
    }
    .content .con_page .a{
        display: block;
        line-height: 30px;
        background: #fff;
        color: #FFA500;
    }
    #go_page{
        width: 30px;
    }
    .content .con_page .page_go .button{
        display:inline-block;
        width:30px;
        height:30px;
        font-size:14px;
        line-height:30px;
        text-align:center;
        border-radius:6px;
        background:#377bd9;
        cursor:pointer;
    }
    .content .con_page .page_go .button:hover{
        background:#1c83d9;
    }
    .button_cli .button{
        display: inline-block;
        width: 22px;
        background: #377BD9;
        border-radius: 4px;
        height: 30px;
        line-height: 25px;
        text-align: center;
    }
    .button_cli{
        float: left;
        text-align: center;
    }
    .now_page_num{
        width: 30px;
    }
    .b{
        display: none;
    }
    .page_go{
        float: right;
        margin-right: 5px;
    }
    .con_page .button{
        margin: 3px 0;
    }
    #now_page_num{
        display: inline-block;
        width: 30px;
    }
}

/*------底部弹窗样式---------*/
.content .bottom_div{
    border:1px solid #4a8bc2;
    position: fixed;
    bottom: 0;

    z-index: 10;
    background-color: #fff;
    display:none;
}
.content .bottom_div .bottom_div_tit{
    height:40px;
    background: #4a8bc2;
}
.bottom_div_con .bottom_div_con_tit{
    margin: 10px auto 2px;
    width: 90%;
}
.content .bottom_div .bottom_div_tit .tit_info{
    line-height:40px;
    margin-left:25px;
    color:#fff;
}
.content .bottom_div .bottom_div_tit .button_style{
    color:#fff;
    float: right;
    display: inline-block;
    width: 65px;
    height: 30px;
    background: #FFAF19;
    margin-right: 20px;
    text-align: center;
    line-height: 30px;
    border-radius: 6px;
    margin-top: 5px;
    cursor: pointer;
    font-weight:bold;
}
.content .bottom_div .bottom_div_tit .button_style:hover{
    background: rgb(231, 151, 0);
}
/*--------左侧栏样式--------*/

.leftcon .left_con_kzt,.leftcon dt{
    display: inline-block;
    width: 100%;
    height: 39px;
    background: #505050;
    line-height: 40px;
    color: #fff;
    font-weight: bold;
    text-indent:50px;
    cursor: pointer;
    border-bottom: 1px solid #6b6b6b;
}
.leftcon .fir_name{
    font-size: 14px;
    color: #fff;
}
.leftcon dt:hover{
    background:#4a8bc2;
}
.leftcon dl dd{
    display:none;
}
.leftcon dl ul li{
    color:#fff;
    font-size: 13px;
    width: 100%;
    height: 39px;
    line-height:40px;
    text-indent:78px;
    background:#505050;
    cursor:pointer;
    font-weight:bold;
    border-bottom: 1px solid #6b6b6b;
}
.leftcon dl ul li:hover{
    background:#636363;
}
.leftcon dl span,.left_con_kzt .icon{
    font-size: 19px;
    color: #9E9E9E;
}
/*--------按钮样式---------*/
.th_button{
    width: 80px;
    height: 26px;
    line-height: 26px;
    background: #FFAF19;
    margin: 0 auto;
    cursor: pointer;
}
.th_button:hover{
    background:rgb(231, 151, 0);
}
.td_change_button{
    width: 55px;
    height: 22px;
    line-height: 22px;
    margin: 0 auto;
    cursor: pointer;
    border: 2px solid rgb(74, 139, 194);
    border-radius: 5px;
    display: inline-block;
}


.con_contral .bu{
    cursor: pointer;
}
/*富文本编辑器样式*/
.ueditor{
    display:inline-block;
    display: inline-block;
    width: 70px;
    height: 30px;
    line-height: 30px;
    background: #FFAF19;
    text-align: Center;
    position: absolute;
    color:#fff;
    font-weight:bold;
    border-radius: 4px;
    cursor:pointer;
}
.ueditor:hover{
    background: rgb(231, 151, 0);
}
.ueditor_a{
    top: 465px;
    right:20px;
}
.ueditor_b{
    top: 465px;
    right: 110px
}
/*弹框*/
#relationCover lable{
    margin-left: 3px;
    display: inline-block;
    width: 60px;
}
#relationCover>input{
    margin: 5px 0;
}
#operation span{
    margin: 15px 0 0 0;
    display: inline-block;
    box-sizing:border-box;
    -moz-box-sizing:border-box; /* Firefox */
    -webkit-box-sizing:border-box; /* Safari */
    width:50%;
    text-align: center;
    background: #4A8BC2;
    height: 35px;
    line-height: 35px;
    color: #fff;
}