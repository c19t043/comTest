@charset "utf-8";

/* ==========================================================================
Author's custom styles
========================================================================== */

/*
* common start
*/
* {
	margin: 0;
	padding: 0;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	font-size: 15px;
}

body {
	width: 100%;
	height: 100%;
	font-size: 1em;
	font-family: Microsoft YaHei;
	background:#fff;
}
label{
	background:#fff;
	display:inline-block;
	color:#666;
}
ul li,
ol li {
	list-style: none;
}
input{
	font-size:14px;
}
input:focus{
	outline:none;
}
img {
	border: 0;
	max-width: 100%;
}

a {
	text-decoration: none;
	color: #222;
}

.float-left {
	float: left;
}

.float-right {
	float: right;
}

.text-left {
	text-align: left;
}

.text-right {
	text-align: right;
}

.text-center {
	text-align: center;
}

.dis-none {
	display: none;
}

.dis-block {
	display: block;
}

.red {
	color: #e95c6a;
}

.white {
	color: white;
}

.black {
	color: black;
}

/*
* common end
*/

#header:before,
#header:after,
#banner:before,
#banner:after,
#container:before,
#container:after,
#footer:before,
#footer:after {
	content: " "; /* 1 */
	display: table; /* 2 */
}

#header:after,
#banner:after,
#container:after,
#footer:after {
	clear: both;
}

/*
* header start
*/
#header {
	width: 100%;
	height: 45px;
	background: #fff;
	text-align: center;
	position: fixed;
	top: 0;
	z-index: 999;
}
#header.doctor_header{
	background: #1cba93;
	color:#fff;
}
#header_left_icon>span{
	display: inline-block;
	width: 30px;
	height: 45px;
	background: url(../images/normalicon/my.png)no-repeat center;
	background-size: 20px;
	font-size: 15px;
}
.header-right.text-center span{
    padding: 2px 12px;
    border-radius: 16px;
    border: 1px solid white;
    font-size: 12px;
}

#header .tit {
	width: 90%;
	margin:0 auto;
}

#header .header-left,#header .headerLeft {
	width: 20%;
	float: left;
	line-height: 45px;
	font-size:30px;
	height:45px;
	color: #c8c8c8;
}

/*#header .header-left p,#header .headerLeft p {*/
	/*background: url("../images/prev_01.png") no-repeat center center;*/
	/*background-size: 25px;*/
	/*height: 45px;*/
/*}*/
#header .header-left p, #header .headerLeft p {
	background: url("../images/normalicon/return.png") no-repeat center center;
	background-size: 20px;
	height: 45px;
}

#header .header-center {
	width: 60%;
	float: left;
	line-height: 45px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	font-size:18px;
}
#header .header-right {
	width: 20%;
    float: right;
    line-height: 45px;
    height:45px;
}
#header .header-right p {
	width: 80%;
	line-height: 24px;
	height: 24px;
	border: 1px solid #1cba93;
	border-radius: 3px;
	margin: 10px 5px;
	background: #1cba93;
	color: #fff;
	font-size: 0.875em;
}

.header_tab {  /*header_tab*/
	width: 50%;
	margin: 6px auto 0px auto;
	height: 30px;
	line-height: 30px;
	clear: both;
	overflow: hidden;
	border: 1px solid #1cba93;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	color: #222;
	font-size: 0.875em;
}
.header_tab .tab_left {
	float: left;
	width: 50%;
}
.header_tab .tab_right {
	float: right;
	width: 50%;
}
.header_tab .select {
	background: #1cba93;
	color: #fff;
}

/*
* header end
*/

/*
* banner start
*/


#banner {
	width: 100%;
	height: 150px;
}
#banner img {
	display: none;
}
/*
* banner end
*/



/*
* container start
*/
#container {
	width: 100%;
	margin:45px 0 50px ;
}



/*
* container end
*/

/*
* footer start
*/
#footer {
	width: 100%;
	height: 45px;
	padding-top: 5px;
	padding-bottom: 8px;
	background: #fff;
	position: fixed;
	bottom: 0;
}
#footer ul a {
	display: block;
}

#footer ul li {
	width: 25%;
	height: 45px;
	display: block;
	float:left;
	text-align: center;
	color: #c8c8c8;
}
#footer ul li.select {
	color: #1cba93;
}
#footer ul li p:last-child {
	font-size: 0.875em;
}

/*
* footer end
*/
