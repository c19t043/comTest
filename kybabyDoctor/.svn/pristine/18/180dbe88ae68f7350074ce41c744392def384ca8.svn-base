/*
弹出提示
*/
#alert-cover{
    position: fixed;
    z-index: 50000;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    left: 0;
    top: 0;
}
#alert-content{
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: -70%;
    text-align: center;
}
#alert-line{
    width: 2px;
    height: 50%;
    background: #1cba93;
    margin: -25% auto 0;
}
#alert-text{
    width: 50%;
    height: 70px;
    background: url("../images/images_login_regist/alert_notice.png") no-repeat;
    background-size:100% 100% ;
    margin: 0 auto;
    padding: 15px 0;
}
#alert-gantanhao{
    margin-bottom:10px ;
}
#alert-text .alert-text-show{
    color: white;
    font-size: 12px;
    padding: 0 15px;
}
#alert-text img{
    width: 12px;
}
/*404错误页面*/
#layer{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 9999;
    width: 100%;
    height: 100%;
    background: #fff;
    color: grey;
    text-align: center;
}
#layer_main{
    margin-top: 35%;
}
#layer_main img{
    width: 25%;
}
#layer_main span{
    display: inline-block;
}
#layer_return{
    padding: 8px 15px 8px 25px;
    background: url(../images/images_login_regist/return_arrow.png)no-repeat left;
    background-size: 22px;
}
#layer_index{
    padding: 8px 22px 8px 15px;
    background: url(../images/images_login_regist/footer_icon01_off.png)no-repeat right;
    background-size: 20px;
}
#layer_p{
    line-height: 45px;
}
/*页尾*/
#footer_nav{
	position: fixed;
	bottom: 0;
	z-index: 999;
	width: 100%;
	height: 50px;
	background: #f7f7f7;
	overflow: hidden;
	-webkit-backface-visibility: hidden;
	text-align: center;
	font-size: 14px;
	color: #909090;
}
.footer_tab_item{
	float: left;
	display: inline-block;
	width: 25%;
	height: 50px;
}
.footer_tab_item.nav-active{
	color: #13ba93;
}
.nav_tab_icon{
	display: block;
	height: 30px;
}
#nav_icon1{
	background: url("../images/menuicon/consulting_dark.png")no-repeat center;
	background-size: 28px;
}
#nav_icon2{
	background: url("../images/menuicon/service_dark.png")no-repeat center;
	background-size: 22px;
}
#nav_icon3{
	background: url("../images/menuicon/circle_dark.png")no-repeat center;
	background-size: 21px;
}
#nav_icon4{
	background: url("../images/menuicon/subscribers_dark.png")no-repeat center;
	background-size: 21px;
}
.nav-active>.nav_text{
	color: #1cba93;
}
.nav-active>#nav_icon1{
	background: url("../images/menuicon/consulting_light.png")no-repeat center;
	background-size: 28px;
}
.nav-active>#nav_icon2{
	background: url("../images/menuicon/service_light.png")no-repeat center;
	background-size: 22px;
}
.nav-active>#nav_icon3{
	background: url("../images/menuicon/circle_light.png")no-repeat center;
	background-size: 21px;
}
.nav-active>#nav_icon4{
	background: url("../images/menuicon/subscribers_light.png")no-repeat center;
	background-size: 21px;
}
