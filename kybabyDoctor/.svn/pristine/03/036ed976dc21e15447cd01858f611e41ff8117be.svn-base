<script>
    $(function () {
        var url_str=window.location.search.substring(1);
        if(url_str==''){

        }else{
            url_str=decodeURIComponent(url_str).split('&');
            var text1=$('.list01 .personal_right').text(url_str[0]);
            if(url_str[1]=='女'){
                $('.list02 .personal_right span:nth-child(2)').addClass('blue');
            }else{
                $('.list02 .personal_right span:nth-child(1)').addClass('blue');
            }
            $('.list03 .personal_right input').val(url_str[2]);
            $('#a').val('好');
            $('#b').val('无');
            $('#c').val('无');
            $('.list08 .personal_right').text('张旭');
            $('.list07 .personal_right input').val(url_str[3]);
            $('.list9 .personal_right input').val('13980123564');
            $('#footer').hide();
        }


        $('.personal_right>span').click(function () {
            $(this).addClass('blue').siblings().removeClass('blue');
        });
        $('#cover a').click(function () {
            var text=$('#searchname').val().trim();
            var arr=[];
            if($('#cover').hasClass('doctor')){
                arr=['周梦','赵英杰','赵莉萍','李正妍','钱琳竣','邹若美','况娅清','范舒玉','黄丽雅','王映蓉','秦夕文','张琳涵','吴燕星','高杰伟','李俊朗','张修杰','曹懿轩','李雄逸','刘鸣'];
            }else if($('#cover').hasClass('patient')){
                arr=['周梦丽','赵英杰','赵莉萍','李正妍','钱琳竣','邹若美','况娅清','范舒玉','黄丽雅','王映蓉','秦夕文','张琳涵','吴燕星','高杰伟','李俊朗','张修杰','曹懿轩','李雄逸','刘鸣'];
            }
            var html='';
            if(text==''){
                for(var j= 0,le=arr.length;j<le;j++){
                    html+='<option value="'+arr[j]+'">'+arr[j]+'</option>';
                }
                $('#name').html(html);
                $('#name>option:nth-child(1)').attr('selected','selected');
            }
            else{
//                $('#name>option').css({display:'none'});
//                $('#name>option').filter(function () {　　　　　　　
//                    return $(this).text().indexOf(text)!=-1;
//                }).css({display:'block'});
                var istrue=0;
                for(var i= 0,len=arr.length;i<len;i++){
                    if(arr[i].indexOf(text)!=-1){
                        html+='<option value="'+arr[i]+'">'+arr[i]+'</option>';
                        istrue=1;
                    }
                }
                if(istrue==0){
                    ale('没有找到相关患者姓名');
                    for(var j= 0,le=arr.length;j<le;j++){
                        html+='<option value="'+arr[j]+'">'+arr[j]+'</option>';
                    }
                }
                $('#name').html(html);
                $('#name>option:nth-child(1)').attr('selected','selected');
            }
        });
        $('#sure>span').click(function () {
            if($('#cover').hasClass('doctor')){
                $('.choosedoctorname').html($('#name>option:selected').val());
            }else if($('#cover').hasClass('patient')){
                $('.choosepatientname').html($('#name>option:selected').val());
            }
            $('#cover').hide();
        });
        $('#footer>p').click(function () {
            var text1=$('.list01 .personal_right').text();
            var text2=$('.list02 .personal_right span.blue').text();
            var text3=$('.list03 .personal_right input').val();
            var text4=$('#a').val();
            var text5=$('#b').val();
            var text6=$('#c').val();
            var text8=$('.list08 .personal_right').text();
            var text7=$('.list07 .personal_right input').val();
            var text9=$('.list9 .personal_right input').val();
            var date=new Date();
            var month=(date.getMonth()+1);
            var day=date.getDate();
            var hour=date.getHours();
            var minute=date.getMinutes();
            var second=date.getSeconds();
            if(month<10){
                month='0'+month;
            }
            if(day<10){
                day='0'+day;
            }
            if(hour<10){
                hour='0'+hour;
            }
            if(minute<10){
                minute='0'+minute;
            }
            if(second<10){
                second='0'+second;
            }
            var timeStr=date.getFullYear()+'-'+month+'-'+day+'&'+hour+':'+minute+':'+second;
//            alert(text1+text2+text3+text4+text5+text6+text7+text8+text9+timeStr);
            ale('确认转诊');
            setTimeout(function () {
                window.location.href='toolreferralrecord.html?'+timeStr+'&'+text1+'&'+text2+'&'+text3+'&'+text8;
            },2000);
        });
    });
function choosepatientname(){
    $('#cover').show().removeClass('doctor').addClass('patient');
    var arr=['周梦丽','赵英杰','赵莉萍','李正妍','钱琳竣','邹若美','况娅清','范舒玉','黄丽雅','王映蓉','秦夕文','张琳涵','吴燕星','高杰伟','李俊朗','张修杰','曹懿轩','李雄逸','刘鸣'];
    var html='';
    for(var j= 0,le=arr.length;j<le;j++){
        html+='<option value="'+arr[j]+'">'+arr[j]+'</option>';
    }
    $('#name').html(html);
    $('#name>option:nth-child(1)').attr('selected','selected');
}
    function choosedoctorname(){
        $('#cover').show().removeClass('patient').addClass('doctor');
        var arr=['周梦','赵英杰','赵莉萍','李正妍','钱琳竣','邹若美','况娅清','范舒玉','黄丽雅','王映蓉','秦夕文','张琳涵','吴燕星','高杰伟','李俊朗','张修杰','曹懿轩','李雄逸','刘鸣'];
        var html='';
        for(var j= 0,le=arr.length;j<le;j++){
            html+='<option value="'+arr[j]+'">'+arr[j]+'</option>';
        }
        $('#name').html(html);
        $('#name>option:nth-child(1)').attr('selected','selected');
    }
</script>
