<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- 套餐订单 -->
	<bean id="mealOrderAction" class="com.java.medicalorgandbusiness.orgsetmeal.action.MealOrderAction"
		scope="prototype">
		<property name="mealOrderService" ref="mealOrderService" />
	</bean>
	
	<!-- 套餐设置 -->
	<bean id="setMealAction" class="com.java.medicalorgandbusiness.orgsetmeal.action.SetMealAction"
		scope="prototype">
		<property name="setMealService" ref="setMealService" />
	</bean>
</beans>