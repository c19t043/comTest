.add_table_bg{/*文本背景框*/
	background-color:#C9D2E4;
	border:0px;
}
.fb_xx_head_tr{/*表单行背景色设为白色*/
	background-color:white;
	height:20px;
	text-align:center;
}
/*table表单数据信息*/
.title_text{/*信息表格标题文字样式*/
    font-family: "宋体";
	font-size: 12px;
	color: #2D3A60;
	font-weight: bold;
	text-indent: 10px;
	text-align:left;
}
.title_bg{/*表格标题背景*/
	background-image: url(../images/main_title_2bg_1.gif);
	background-repeat: repeat-x;
	vertical-align:left;
	height: 18px;
}
.fb_xx_head{/*列表标题样式*/
	height:20px;
	color: #000000;
	text-align: center;
	vertical-align: middle;
	font-family: "宋体";
	font-size: 12px;
	background-color: #F3F4F6;
}
.fb_xx_head_tr{/*表单行背景色设为白色*/
	background-color:white;
	height:20px;
	text-align:center;
}
/*Tab标签*/
.add_title_bg{/*Tab线条*/
	border-top-width: 6px;
	border-top-style: solid;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: none;
	border-top-color:#81A1DA;
}
.tab01{/*当前显示TABLE*/
	background-image: url(../images/epp_tab_01.gif);
	background-repeat: no-repeat;
	text-align: center;
	vertical-align: bottom;
	height: 22px;
	width:105px;
	color: #422B10;
	cursor: hand;
}
.tab02{/*未选中TABLE*/
	background-image: url(../images/epp_tab_02.gif);
	background-repeat: no-repeat;
	text-align: center;
	vertical-align: bottom;
	height: 22px;
	width: 105px;
	color: #422B10;
	cursor: hand;
}




