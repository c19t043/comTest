	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean id="phPrenatalFollowRecordFirstAction" class="com.java.publichealth.productionvisit.action.PhPrenatalFollowRecordFirstAction" scope="prototype">
