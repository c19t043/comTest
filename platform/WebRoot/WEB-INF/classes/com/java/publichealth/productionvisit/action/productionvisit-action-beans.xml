	</bean>
	<bean id="phPostpartumFollowRecordAction" class="com.java.publichealth.productionvisit.action.PhPostpartumFollowRecordAction" scope="prototype">
		<property name="phPostpartumFollowRecordService" ref="phPostpartumFollowRecordServiceImpl" />
	</bean>
</beans>