<!DOCTYPE html>
<html>
<head>
	<title>儿保开放设置</title>
	<meta charset="utf-8">
	<meta name="renderer" content="webkit" />
	<script src="js/jquery-1.11.3.min.js"></script>
	<script src="js/date/WdatePicker.js"></script>
	<script src="js/basic.js"></script>
	<script src="js/common.js"></script>
	<script src="js/orgbo_childcare_manage.js"></script>
	<link rel="stylesheet" type="text/css" href="css/common.css">
	<link rel="stylesheet" type="text/css" href="css/iconfont/iconfont.css">
	<link rel="stylesheet" type="text/css" href="css/orgbo_childcare_manage.css">
</head>
<body>
<input type="hidden" value="false" id="bt_flag"/>
<div id="headcon" class="headcon">

</div>

<div class="middlecon">
	<div id="leftcon" class="leftcon">

	</div>

	<div id="rightcon" class="rightcon">
		<div class="content">
			<div id="con_position" class="con_position">
				<h1 id="con_tit" class="con_tit"></h1>
				<div id="con_meu" class="con_meu">
					<span>首页</span>
					<span>/</span>
					<span class="first"></span>
					<span>/</span>
					<span class="second"></span>
				</div>
				<div class="con_contral">
					<label>机构名称：</label><input style="width: 120px" id="hospitalLname" type="text">
                    <label>开放日期：</label><input style="width: 120px" onfocus="WdatePicker()" id="open_Date" type="text">
                    <span onclick="search_click()" class="bu">查找</span>
				</div>
				<table id="table_title" class="table_title">

				</table>
			</div>
			<div class="table_h">
				<table id="table_content" class="table_content">

				</table>
			</div>
			<div class="con_page">
				<span class="a">共&nbsp;<span id="con_num" class="con_num"></span>&nbsp;条数据</span>
				<div class="button_cli">
					<span class="button" onclick="page_click('default','1')">首页</span>
					<span class="button" onclick="page_click('default','prev')" >上一页</span>
					<span class="num" id="now_page_num" >1</span>
					<span class="button" onclick="page_click('default','next')">下一页</span>
					<span class="button" onclick="page_click('default','end')">尾页</span>
				</div>
				<div class="page_go">
					<span class="b">共&nbsp;<span id="page_num" class="page_num"></span>&nbsp;页</span>
					<span class="c">跳转到&nbsp;<input id="go_page" type="number" value="1" min="1">&nbsp;页</span>
					<span class="button" onclick="page_click_go()">Go</span>
				</div>
			</div>
			
			<div id="bottom_div" class="bottom_div">
				<div class="bottom_div_tit">
					 <span class="tit_info"></span>
					 <span id="button_add_click" onclick="saveOrUpdate()" class="button_style button_add">确认</span>
					<span id="button_cancel_click" class="button_style button_cancel">取消</span>
				</div>
				<div id="bottom_div_con" class="bottom_div_con">
					<form id="data_form" action="" method="post" enctype="multipart/form-data">
			            <div class="fj">
			            	<input id="keyId" type="hidden" name="organChildcareOpenResources.id"/>
			            	<input id="orgId" type="hidden"/>
							<label>社区机构：</label>
							<select id="hospitalBasicInfo" name="organChildcareOpenResources.hospitalBasicInfo.id">
							</select>
							
							<label>开放日期：</label>
							<input id="openDate" type="text" name="organChildcareOpenResources.openDate" onfocus="WdatePicker()" readonly="readonly">
							
							<label>开始时间：</label>
							<input id="openStartTime" type="text" name="organChildcareOpenResources.openStartTime" 
									onfocus="WdatePicker({isShowClear:true,dateFmt:'HH:mm',readOnly:true})" readonly="readonly">
							<!-- <select  id="openStartTime" name="organChildcareOpenResources.openStartTime">
								<option>08:00</option>
								<option>08:30</option>
								<option>09:00</option>
								<option>09:30</option>
								<option>10:00</option>
								<option>13:00</option>
								<option>13:30</option>
								<option>14:00</option>
								<option>14:30</option>
							</select> -->
												
							<label>结束时间：</label>
							<input id="openEndTime" type="text" name="organChildcareOpenResources.openEndTime" 
									onfocus="WdatePicker({dateFmt:'HH:mm'})" readonly="readonly">
							<!-- <select id="openEndTime" name="organChildcareOpenResources.openEndTime">
								<option>11:00</option>
								<option>11:30</option>
								<option>12:00</option>
								<option>13:00</option>
								<option>13:30</option>
								<option>14:00</option>
								<option>14:30</option>
								<option>15:00</option>
								<option>16:00</option>
								<option>16:30</option>
								<option>17:00</option>
								<option>17:30</option>
							</select> -->
			            </div>
			            <div class="fj">
			            	<label>休息开始时间：</label>
			            	<input id="restStartTime" type="text" name="organChildcareOpenResources.restStartTime" 
									onfocus="WdatePicker({dateFmt:'HH:mm'})" readonly="readonly">
							<!-- <select  id="restStartTime" name="organChildcareOpenResources.restStartTime">
								<option>12:00</option>
								<option>11:30</option>
							</select> -->
							
			            	<label>休息结束时间：</label>
			            	<input id="restEndTime" type="text" name="organChildcareOpenResources.restEndTime" 
									onfocus="WdatePicker({dateFmt:'HH:mm'})" readonly="readonly">
							<!-- <select  id="restEndTime" name="organChildcareOpenResources.restEndTime">
								<option>11:30</option>
								<option>12:00</option>
								<option>12:30</option>
								<option>13:00</option>
								<option>13:30</option>
								<option>14:00</option>
								<option>14:30</option>
							</select> -->
							
							<label>时间分割值：</label>
							<select id="timeDivisionValue" name="organChildcareOpenResources.timeDivisionValue">
								<option>15</option>
								<option>20</option>
								<option>30</option>
								<option>60</option>
							</select>
							
							<label>是否可用：</label>
							<select id="isAvailable" name="organChildcareOpenResources.isAvailable">
								<option>Y</option>
								<option>N</option>
							</select>
			            </div>
			             <div class="fj">
			             	<label>是否收费：</label>
							<select id="isMoney" name="organChildcareOpenResources.isMoney" style="width: 70px">
								<option value="N">免费</option>
								<option value="Y">收费</option>
							</select>
							<label>坐诊医生：</label>
							<span id="show_doctor"></span>
							<input id="doctorIds" name="organChildcareOpenResources.doctorIds" type="hidden"/>
							<span onclick="chooseObj('doctor')" class="dzstyle">修改</span>
			            	<label>时间开放需求：</label>
							<select id="timeDivisionNeed" name="organChildcareOpenResources.timeDivisionNeed" onchange="changeNeed(this)">
								<option>时间点</option>
								<option>时间段</option>
							</select>
							
							<label>时间开放类型：</label>
							<select id="timeDivisionType" name="organChildcareOpenResources.timeDivisionType">
								<option>null</option>
								<option>普卡</option>
								<option>金卡</option>
							</select>
			            </div>
			            <div class="fj">
			            	<label>是否设置预约截止时间：</label>
							<select id="isUseDeadline" name="organChildcareOpenResources.isUseDeadline" onchange="changeDeadline(this)">
								<option value="N">N</option>
								<option value="Y">Y</option>
							</select>
							<span style="display: none" id="deadlineObj">
								<label>预约截止时间：</label>
								<input id="deadline" type="text" name="organChildcareOpenResources.deadline" 
									onfocus="WdatePicker({dateFmt:'HH:mm:ss'})" readonly="readonly">
							</span>
			            </div>
			             <div class="fj" id="openNum" style="display: none">
			            	<label>普通窗口开放资源：</label>
							<input id="generalSurplusNum" type="text" name="organChildcareOpenResources.generalNum" placeholder="输入整数">
							
							<label>绿色通道开放资源：</label>
							<input id="greenChannelSurplusNum" type="text" name="organChildcareOpenResources.greenChannelNum" placeholder="输入整数">
			            </div>
		          	</form>
				</div>
	          	<!-- 儿保开放明细 -->
	          	<table id="node_table_title" class="table_title">
				</table>
				<div class="table_h" style="overflow:auto;">
					<table id="node_table_content" class="table_content" border="1" >
						<tbody id="node_Tbody"></tbody>
					</table>
				</div>
			</div>
						<!-- 选择擅长领域 -->
			<div id="bg"></div>
			<div class="boxZhezhao" style="display:none">
			    <h2>选择<a href="javascript:hideMask()" class="close">关闭</a></h2>
			    <table class="table_title" id="titalTable">
					
				</table>
			    <div class="list">
			    	<table id="selectContent" class="table_content"></table>
			    </div>
			</div>
		</div>	
	</div>
</div>

  </body>
</html>