	@Override
	public String kyInterfaceInvoke(OrderInfo updateOrder) {
		Session session = this.getHibernateTemplate().getSessionFactory().openSession();
		String sql = "SELECT kyid FROM kyorder2myorder WHERE id = :id";
		SQLQuery createSQLQuery = session.createSQLQuery(sql);
		createSQLQuery.setLong("id", updateOrder.getId());
		Object uniqueResult = createSQLQuery.uniqueResult();
		if(uniqueResult==null) return null;
		return uniqueResult.toString();
	}

